<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!--    Sequence for Delivery Receipt Orders-->
        <record id="seq_delivery_receipt" model="ir.sequence">
            <field name="name">Delivery Receipt Orders</field>
            <field name="code">delivery.receipt.orders</field>
            <field name="prefix">DR/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!--    Sequence for Decanting Orders Process-->
        <record id="seq_decanting_orders_process" model="ir.sequence">
            <field name="name">Automation Decanting Orders Process</field>
            <field name="code">automation.decanting.orders.process</field>
            <field name="prefix">ADP/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>
