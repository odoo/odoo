#. module: via_suite_base
#: model:ir.ui.view,arch_db:via_suite_base.web_login_viasuite
msgid "Access Denied"
msgstr "Acceso Denegado"

#. module: via_suite_base
#: model:ir.ui.view,arch_db:via_suite_base.web_login_viasuite
msgid "User Not Found"
msgstr "Usuario No Encontrado"

#. module: via_suite_base
#: model:ir.ui.view,arch_db:via_suite_base.web_login_viasuite
msgid "Account Disabled"
msgstr "Cuenta Deshabilitada"

#. module: via_suite_base
#: model:ir.ui.view,arch_db:via_suite_base.web_login_viasuite
msgid "You are trying to access a different tenant than the one configured for your account."
msgstr "Usted está intentando acceder a un inquilino diferente al configurado en su cuenta."

#. module: via_suite_base
#: model:ir.ui.view,arch_db:via_suite_base.web_login_viasuite
msgid "Your account was authenticated successfully, but you are not registered in this system yet."
msgstr "Su cuenta fue autenticada con éxito, pero aún no está registrado en este sistema."

#. module: via_suite_base
#: model:ir.ui.view,arch_db:via_suite_base.web_login_viasuite
msgid "Your account exists but is currently disabled. Please contact support."
msgstr "Su cuenta existe pero está actualmente deshabilitada. Por favor, póngase en contacto con el soporte."

#. module: via_suite_base
#: model:ir.ui.view,arch_db:via_suite_base.web_login_viasuite
msgid "An unexpected error occurred during authentication."
msgstr "Ocurrió un error inesperado durante la autenticación."

#. module: via_suite_base
#: model:ir.ui.view,arch_db:via_suite_base.web_login_viasuite
msgid "What to do?"
msgstr "¿Qué hacer?"

#. module: via_suite_base
#: model:ir.ui.view,arch_db:via_suite_base.web_login_viasuite
msgid "Ensure you're using the correct login URL."
msgstr "Asegúrese de estar utilizando la URL de inicio de sesión correcta."

#. module: via_suite_base
#: model:ir.ui.view,arch_db:via_suite_base.web_login_viasuite
msgid "Verify your account status with the administrator."
msgstr "Verifique el estado de su cuenta con el administrador."

#. module: via_suite_base
#: model:ir.ui.view,arch_db:via_suite_base.web_login_viasuite
msgid "Support:"
msgstr "Soporte:"

#. module: via_suite_base
#: model:ir.ui.view,arch_db:via_suite_base.web_login_viasuite
msgid "Back to Login"
msgstr "Volver al Inicio de Sesión"
