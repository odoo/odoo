#. module: via_suite_base
#: model:ir.ui.view,arch_db:via_suite_base.web_login_viasuite
msgid "Access Denied"
msgstr "Acesso Negado"

#. module: via_suite_base
#: model:ir.ui.view,arch_db:via_suite_base.web_login_viasuite
msgid "User Not Found"
msgstr "Usuário Não Encontrado"

#. module: via_suite_base
#: model:ir.ui.view,arch_db:via_suite_base.web_login_viasuite
msgid "Account Disabled"
msgstr "Conta Desativada"

#. module: via_suite_base
#: model:ir.ui.view,arch_db:via_suite_base.web_login_viasuite
msgid "Service Suspended"
msgstr "Ambiente Suspenso"

#. module: via_suite_base
#: model:ir.ui.view,arch_db:via_suite_base.web_login_viasuite
msgid "This tenant environment is currently inactive. Please contact your administrator."
msgstr "Este ambiente está atualmente inativo. Por favor entre em contato com o administrador."

#. module: via_suite_base
#: model:ir.ui.view,arch_db:via_suite_base.web_login_viasuite
msgid "You are trying to access a different tenant than the one configured for your account."
msgstr "Você está tentando acessar um tenant diferente do configurado em sua conta."

#. module: via_suite_base
#: model:ir.ui.view,arch_db:via_suite_base.web_login_viasuite
msgid "Your account was authenticated successfully, but you are not registered in this system yet."
msgstr "Sua conta foi autenticada com sucesso, mas você ainda não foi cadastrado neste sistema."

#. module: via_suite_base
#: model:ir.ui.view,arch_db:via_suite_base.web_login_viasuite
msgid "Your account exists but is currently disabled. Please contact support."
msgstr "Sua conta existe no sistema mas está atualmente desativada. Por favor entre em contato com o suporte."

#. module: via_suite_base
#: model:ir.ui.view,arch_db:via_suite_base.web_login_viasuite
msgid "An unexpected error occurred during authentication."
msgstr "Ocorreu um erro inesperado durante a autenticação."

#. module: via_suite_base
#: model:ir.ui.view,arch_db:via_suite_base.web_login_viasuite
msgid "What to do?"
msgstr "O que fazer?"

#. module: via_suite_base
#: model:ir.ui.view,arch_db:via_suite_base.web_login_viasuite
msgid "Ensure you're using the correct login URL."
msgstr "Verifique se está acessando a URL de login correta."

#. module: via_suite_base
#: model:ir.ui.view,arch_db:via_suite_base.web_login_viasuite
msgid "Verify your account status with the administrator."
msgstr "Verifique o status da sua conta com o administrador."

#. module: via_suite_base
#: model:ir.ui.view,arch_db:via_suite_base.web_login_viasuite
msgid "Support:"
msgstr "Suporte:"

#. module: via_suite_base
#: model:ir.ui.view,arch_db:via_suite_base.web_login_viasuite
msgid "Back to Login"
msgstr "Voltar para Login"
