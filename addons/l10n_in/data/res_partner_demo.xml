    <!-- Registered Customer -->
    <record id="res_partner_registered_supplier" model="res.partner">
        <field name="name">Registered Supplier</field>
        <field eval="[(6, 0, [ref('l10n_in.res_partner_category_registered')])]" name="category_id"/>
