<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <record id="account_common_report_view_inherit_l10n_in" model="ir.ui.view">
        <field name="name">Common Report.inherit.l10n_in</field>
        <field name="model">account.print.journal</field>
        <field name="inherit_id" ref="account.account_common_report_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='company_id']" position="after">
                <field name="l10n_in_unit_id" domain="[('company_l10n_in_unit_id', '=', company_id)]" options="{'no_create': True}" groups="l10n_in.group_multi_operating_unit"/>
            </xpath>
        </field>
    </record>
</odoo>
