<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- These are shells to allow for assigning printers -->
    <data>
        <template id="report_shipping_labels"/>
        <template id="report_shipping_docs"/>
        <record id="action_report_shipping_labels" model="ir.actions.report">
            <field name="name">Shipping Labels</field>
            <field name="model">stock.picking</field>
            <field name="report_name">stock_delivery.report_shipping_labels</field>
            <field name="print_report_name">'Shipping-Label-%s' % object.name</field>
            <field name="binding_model_id" ref="stock.model_stock_picking"/>
            <field name="binding_type">report</field>
        </record>
        <record id="action_report_shipping_docs" model="ir.actions.report">
            <field name="name">Shipping Documents</field>
            <field name="model">stock.picking</field>
            <field name="report_name">stock_delivery.report_shipping_docs</field>
            <field name="print_report_name">'Shipping-Document-%s' % object.name</field>
            <field name="binding_model_id" ref="stock.model_stock_picking"/>
            <field name="binding_type">report</field>
        </record>
    </data>
</odoo>
