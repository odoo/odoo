<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Fiscal Position Templates -->
        <record id="l10n_kz_fiscal_position_template_national" model="account.fiscal.position.template">
            <field name="sequence">1</field>
            <field name="name">National</field>
            <field name="chart_template_id" ref="l10nkz_chart_template"/>
            <field name="auto_apply" eval="True"/>
            <field name="country_id" ref="base.kz"/>
        </record>

        <record id="l10n_kz_fiscal_position_template_eeu" model="account.fiscal.position.template">
