"\n"
"                %if object.origin:\n"
"                    ${object.company_id.name}: Vi er glade for at kunne informere dig om, at din ordre nummer ${object.origin} er blevet afsendt.\n"
"                %else:\n"
"                    ${object.company_id.name}: Vi er glade for at kunne informere dig om, at din ordre er blevet afsendt.\n"
"                %endif\n"
"                %if object.carrier_tracking_ref:\n"
"                    Dit sporings nummer er ${object.carrier_tracking_ref}.\n"
"                %endif\n"
"            "

