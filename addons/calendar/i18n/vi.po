"event_id.description :\n"
"                                                                <div style="
"\"font-family: Lucica Grande', Ubuntu, Arial, Verdana, sans-serif;\">\n"
"                                                                    : "
"${object.event_id.description}\n"
"                                                                </div>\n"
"                                                            % endif\n"
"                                                        </td>\n"
"                                                    </tr>\n"
"                                                                                                \n"
"                                                    <tr>\n"
"                                                        <td style=\"vertical-"
"align:top;\">\n"
"                                                            % if not object."
"event_id.allday and object.event_id.duration:\n"
"                                                                <div style="
"\"height:auto; width: 120px; background : #CCCCCC; font-family: Lucica "
