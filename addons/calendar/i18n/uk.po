"                                                            : ${('%dH%02d' % (object.event_id.duration,(object.event_id.duration*60)%60))}\n"
"                                                        </div>\n"
"                                                    % endif\n"
"                                                </td>\n"
"                                            </tr>                                                \n"
"                                        <tr style=\" height: 30px;\">\n"
"                                            <td style=\"height: 25px;width: 120px; background : # CCCCCC; font-family: Lucida Grande', Ubuntu, Arial, Verdana, sans-serif;\">\n"
