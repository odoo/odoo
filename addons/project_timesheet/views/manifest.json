{
    "manifest_version": 2,
    "name": "Odoo extension for Project Timesheet",
    "version": "9.0",
    "description": "This extension will allows users to create Project Timesheet entries in speedy way with asynchronous mode.",
    "icons": {
        "16": "static/src/img/logo_16.png",
        "19": "static/src/img/logo_19.png",
        "38": "static/src/img/logo_38.png",
        "48": "static/src/img/logo_48.png",
        "128": "static/src/img/logo_128.png"
    },
    "background": {
        "scripts": [
            "static/src/js/background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "static/src/img/logo_16.png",
        "default_popup": "index.html"
    },
    "permissions": [
        "http://*/",
        "https://*/",
        "activeTab"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
