<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <!-- Twilio Account SID -->
        <record id="param_twilio_account_sid" model="ir.config_parameter">
            <field name="key">twilio_account_sid</field>
            <field name="value">your_twilio_account_sid_here</field>
        </record>

        <!-- Twilio Auth Token -->
        <record id="param_twilio_auth_token" model="ir.config_parameter">
            <field name="key">twilio_auth_token</field>
            <field name="value">your_twilio_auth_token_here</field>
        </record>

        <!-- Twilio Phone Number -->
        <record id="param_twilio_phone_number" model="ir.config_parameter">
            <field name="key">twilio_phone_number</field>
            <field name="value">your_twilio_phone_number_here</field>
        </record>
    </data>
</odoo>
