<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_website_enterprise_config_settings" model="ir.ui.view">
        <field name="name">website enterprise settings</field>
        <field name="model">website.config.settings</field>
        <field name="inherit_id" ref="website.view_website_config_settings"/>
        <field name="arch" type="xml">
            <group name="social" position="after">
                <group string="Extra">
                    <field name="module_website_form_editor" class="oe_inline" widget="upgrade_radio"/>
                    <field name="cta_blocks" class="oe_inline" widget="upgrade_radio"/>
                    <field name="module_website_version" class="oe_inline" widget="upgrade_radio"/>
                </group>
            </group>
        </field>
    </record>
</odoo>
