<?xml version="1.0" encoding="utf-8"?>
<odoo>

<template id="s_website_form_aside" name="Form Aside">
    <section class="s_website_form_aside pt64 pb64" data-snippet="s_website_form_aside">
        <div class="container">
            <div class="row">
                <div class="col-lg-7 order-lg-0 s_website_form" style="order: 1" data-vcss="001" data-snippet="s_website_form" data-name="Form">
