<?xml version="1.0" encoding="utf-8"?>
<odoo>

<template id="s_references_carousel" name="References Carousel">
    <section class="s_references_carousel o_cc o_cc1 pt64 pb64">
        <div class="container-fluid">
            <h2 style="text-align: center;">Trusted references</h2>
            <p class="lead" style="text-align: center;">We are in good company.</p>
            <div class="row s_col_no_resize s_references_carousel_slider_wrapper">
                <div class="s_references_carousel_slider o_not_editable col-12" data-name="Slider">
                    <img src="/web/image/website.s_reference_carousel_demo_image_1" class="s_references_carousel_item o_editable_media img img-fluid" style="--position: 1;" loading="eager" alt=""/>
                    <img src="/web/image/website.s_reference_carousel_demo_image_2" class="s_references_carousel_item o_editable_media img img-fluid" style="--position: 2;" loading="eager" alt=""/>
                    <img src="/web/image/website.s_reference_carousel_demo_image_3" class="s_references_carousel_item o_editable_media img img-fluid" style="--position: 3;" loading="eager" alt=""/>
                    <img src="/web/image/website.s_reference_carousel_demo_image_4" class="s_references_carousel_item o_editable_media img img-fluid" style="--position: 4;" loading="eager" alt=""/>
                    <img src="/web/image/website.s_reference_carousel_demo_image_5" class="s_references_carousel_item o_editable_media img img-fluid" style="--position: 5;" loading="eager" alt=""/>
                    <img src="/web/image/website.s_reference_carousel_default_image_6" class="s_references_carousel_item o_editable_media img img-fluid" style="--position: 6;" loading="eager" alt=""/>
                    <img src="/web/image/website.s_reference_carousel_default_image_7" class="s_references_carousel_item o_editable_media img img-fluid" style="--position: 7;" loading="eager" alt=""/>
                </div>
            </div>
        </div>
    </section>
</template>

<!-- Assets -->
<asset id="website.s_references_carousel_000_scss" name="References Carousel 000 SCSS">
    <bundle>web.assets_frontend</bundle>
    <path>website/static/src/snippets/s_references_carousel/000.scss</path>
</asset>

</odoo>
