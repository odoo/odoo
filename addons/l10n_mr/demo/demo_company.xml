<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="partner_demo_company_mr" model="res.partner">
        <field name="name">MR Company</field>
        <field name="vat">MR12345634</field>
        <field name="street">Route Nouadhibou</field>
        <field name="city">Nouakchott</field>
        <field name="country_id" ref="base.mr"/>
        <field name="zip">100000</field>
        <field name="phone">+222 2123 1998</field>
        <field name="email">info@company.mrexample.com</field>
        <field name="website">www.mrexample.com</field>
    </record>

    <record id="demo_company_mr" model="res.company">
        <field name="name">MR Company</field>
        <field name="partner_id" ref="partner_demo_company_mr"/>
    </record>

    <record id="demo_bank_mr" model="res.partner.bank">
        <field name="acc_number">MR1300020001010000123456753</field>
        <field name="partner_id" ref="partner_demo_company_mr"/>
        <field name="company_id" ref="demo_company_mr"/>
    </record>

    <function model="res.company" name="_onchange_country_id">
        <value eval="[ref('demo_company_mr')]"/>
    </function>

    <function model="res.users" name="write">
        <value eval="[ref('base.user_root'), ref('base.user_admin'), ref('base.user_demo')]"/>
        <value eval="{'company_ids': [(4, ref('l10n_mr.demo_company_mr'))]}"/>
    </function>

    <function model="account.chart.template" name="try_loading">
        <value eval="[]"/>
        <value>mr</value>
        <value model="res.company" eval="obj().env.ref('l10n_mr.demo_company_mr')"/>
        <value name="install_demo" eval="True"/>
    </function>
</odoo>
