      <div
        t-ref="dragOverlay"
        class="o-we-table-drag-drop"
        t-att-style="
          'top:' + overlayRect.top + 'px; ' +
          'left:' + overlayRect.left + 'px; ' +
          'width:' + overlayRect.width + 'px; ' +
          'height:' + overlayRect.height + 'px;'
        "
      >
      </div>
    </div>
  </t>
</templates>
