<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="tax_group_gv_stopaji" model="account.tax.group">
            <field name="name">Withholding Income Tax</field>
            <field name="country_id" ref="base.tr"/>
        </record>

        <record id="tax_group_kv_stopaji" model="account.tax.group">
            <field name="name">Withholding Rent Tax</field>
            <field name="country_id" ref="base.tr"/>
        </record>

        <record id="tax_group_vat_full" model="account.tax.group">
            <field name="name">18% VAT</field>
            <field name="country_id" ref="base.tr"/>
            <field name="property_tax_payable_account_id"
                   search="[('code', '=', '360000')]" />
            <field name="property_tax_receivable_account_id"
                   search="[('code', '=', '190000')]" />
            <field name="property_advance_tax_payment_account_id"
                   search="[('code', '=', '193000')]" />
        </record>

        <record id="tax_group_vat_discounted" model="account.tax.group">
            <field name="name">8% VAT</field>
            <field name="country_id" ref="base.tr"/>
            <field name="property_tax_payable_account_id"
                   search="[('code', '=', '360000')]" />
            <field name="property_tax_receivable_account_id"
                   search="[('code', '=', '190000')]" />
            <field name="property_advance_tax_payment_account_id"
                   search="[('code', '=', '193000')]" />
        </record>

        <record id="tax_group_vat_double_discount" model="account.tax.group">
            <field name="name">1% VAT</field>
            <field name="country_id" ref="base.tr"/>
            <field name="property_tax_payable_account_id"
                   search="[('code', '=', '360000')]" />
            <field name="property_tax_receivable_account_id"
                   search="[('code', '=', '190000')]" />
            <field name="property_advance_tax_payment_account_id"
                   search="[('code', '=', '193000')]" />
        </record>

        <record id="tax_group_not_subjected" model="account.tax.group">
            <field name="name">Not subjected to VAT</field>
            <field name="country_id" ref="base.tr"/>
        </record>

        <record id="tax_group_zero_vat" model="account.tax.group">
            <field name="name">VAT Exception</field>
            <field name="country_id" ref="base.tr"/>
        </record>

        <record id="tax_group_vat_partial_withdrawal" model="account.tax.group">
            <field name="name">Withholding VAT</field>
            <field name="country_id" ref="base.tr"/>
        </record>
    </data>
</odoo>
