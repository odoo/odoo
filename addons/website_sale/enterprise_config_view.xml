<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_website_sale_enterprise_config_settings" model="ir.ui.view">
        <field name="name">website sales enterprise settings</field>
        <field name="model">website.config.settings</field>
        <field name="inherit_id" ref="website_sale.website_config_settings_view_form"/>
        <field name="arch" type="xml">
            <field name="cdn_filters" position="after">
                <field name="module_delivery_fedex" class="oe_inline" widget="upgrade_boolean"/>
                <field name="module_delivery_ups" class="oe_inline" widget="upgrade_boolean"/>
                <field name="module_delivery_usps" class="oe_inline" widget="upgrade_boolean"/>
                <field name="module_sale_ebay" class="oe_inline" widget="upgrade_boolean"/>
            </field>
        </field>
    </record>
</odoo>
