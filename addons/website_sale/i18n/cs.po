"if=\"product_description_line\" t-out=\"product_description_line\" "
"style=\"margin: 4px 0 0;\"/>\n"
"                                                            </t>\n"
"                                                        </div>\n"
"                                                    </t>\n"
"                                                </t>\n"
"                                            </t>\n"
"                                            <span style=\"display: inline-"
"block;  margin-top: 4px; padding: 4px 8px; border-radius: 20px; background-"
"color: #e9ecef; color: #454748; font-size: 12px;\">\n"
