<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <t t-name="website_sale.WarningNotification">
        <div
            class="toast show border-warning-subtle bg-warning-subtle"
            role="alert"
            aria-live="assertive"
            aria-atomic="true"
        >
            <div class="toast-body">
                <i class="fa fa-warning pe-2"/>
                <t t-out="this.props.warning_message"/>
            </div>
        </div>
    </t>

</templates>
