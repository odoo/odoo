<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
    <t t-name="web.HeaderSetting">
        <div t-att-class="this.classNames">
            <label t-if="this.labelString" class="o_form_label" t-att-for="this.props.fieldId">
                <t t-esc="this.labelString"/>
            </label>
            <div class="input-group align-items-baseline w-auto o_field_highlight">
                <t t-slot="fieldSlot"/>
                <t t-slot="default"/>
            </div>
        </div>
    </t>
</templates>
