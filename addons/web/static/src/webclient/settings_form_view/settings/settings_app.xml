<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
    <t t-name="web.SettingsApp">
        <div class="app_settings_block" t-if="this.props.selectedTab === this.props.key or this.state.search.value.length !== 0" t-att-string="this.props.string" t-att-data-key="this.props.key" t-ref="settingsApp">
            <div class="settingSearchHeader h4" t-if="this.state.search.value.length !== 0" role="search">
                <img class="icon" t-att-src="this.props.imgurl" alt="Search"></img>
                <span class="appName"><t t-esc="this.props.string"/></span>
            </div>
            <t t-slot="default"/>
        </div>
    </t>
</templates>
