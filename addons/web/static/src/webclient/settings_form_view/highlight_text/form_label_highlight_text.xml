<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
    <t t-name="web.FormLabelHighlightText">
        <label class="o_form_label" t-att-for="this.props.id" t-att-class="this.className">
            <HighlightText originalText="this.props.string"/>
            <t t-if="this.upgradeEnterprise">
                <span class="badge text-bg-primary oe_inline o_enterprise_label">Enterprise</span>
            </t>
        </label>
    </t>
</templates>
