<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <t t-name="web.StatusBarDropdownItems">
        <t t-foreach="visibleSlotNames" t-as="slot" t-key="slot">
            <DropdownItem class="'o_statusbar_button_dropdown_item'">
                <t t-slot="{{ slot }}" />
            </DropdownItem>
        </t>
    </t>

</templates>
