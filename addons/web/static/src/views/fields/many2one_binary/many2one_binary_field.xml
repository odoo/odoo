<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <t t-name="web.Many2OneBinaryField">
        <Many2XBinary
            name="props.name"
            readonly="props.readonly"
            record="props.record"
            numberOfFiles="1"
            acceptedFileExtensions="props.acceptedFileExtensions"
            className="props.className"
            files="file"
            onFileUploaded.bind="onFileUploaded"
            onFileRemove.bind="onFileRemove"
        />
    </t>

</templates>
