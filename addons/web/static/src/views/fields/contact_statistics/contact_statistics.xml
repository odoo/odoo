<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <t t-name="web.ContactStatisticsField">
        <div class="d-inline-flex flex-wrap gap-3">
            <span t-foreach="list" t-as="data" t-key="data_index" t-att-aria-label="data.label" t-att-title="data.label" t-att-data-tooltip="data.label">
                <i t-attf-class="fa fa-fw me-1 pe-none {{data.iconClass}}" role="img"/>
                <t t-out="data.value"/>
            </span>
        </div>
    </t>

</templates>
