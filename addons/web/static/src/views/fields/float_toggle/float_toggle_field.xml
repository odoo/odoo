<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

    <t t-name="web.FloatToggleField">
        <span t-if="this.props.readonly and !this.props.disableReadOnly" t-esc="this.formattedValue" />
        <button t-else="" class="o_field_float_toggle" t-on-click="this.onChange"><t t-esc="this.formattedValue" /></button>
    </t>

</templates>
