<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">

    <t t-name="web.Field">
        <div t-att-name="this.props.name" t-att-class="this.classNames" t-att-style="this.props.style" t-att-data-tooltip-template="this.tooltip and 'web.FieldTooltip'" t-att-data-tooltip-info="this.tooltip">
            <t t-component="this.field.component" t-props="this.fieldComponentProps"/>
        </div>
    </t>

</templates>
