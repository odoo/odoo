<?xml version="1.0" encoding="UTF-8"?>
<templates>
    <t t-name="web.FileUploadProgressContainer">
        <t t-foreach="Object.values(this.props.fileUploads)" t-as="fileUpload" t-key="fileUpload.id">
            <t t-if="!this.props.shouldDisplay || this.props.shouldDisplay(fileUpload)" t-component="this.props.Component" fileUpload="fileUpload" selector="this.props.selector"/>
        </t>
    </t>
</templates>
