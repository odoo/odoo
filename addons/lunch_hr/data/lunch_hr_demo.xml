<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="partner_farm_1" model="res.partner">
            <field name="name">Farm 1</field>
        </record>

        <record id="partner_farm_2" model="res.partner">
            <field name="name">Farm 2</field>
        </record>

        <record id="partner_brussels" model="res.partner">
            <field name="name">Brussels</field>
        </record>

        <record id="hr.employee_admin" model="hr.employee">
            <field name="address_id" ref="partner_farm_1"/>
        </record>

        <record id="lunch.supplier_pizza_inn" model="lunch.supplier">
            <field name="available_location_ids" eval="[
                (6, 0, [ref('lunch_hr.partner_farm_1'), ref('lunch_hr.partner_farm_2')]),
                ]"/>
        </record>

    </data>
</odoo>
