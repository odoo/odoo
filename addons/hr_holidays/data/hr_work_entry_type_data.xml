<?xml version="1.0" encoding="utf-8"?>
<odoo><data noupdate="1">
<!-- Global Leave Types -->
    <!-- Paid Time Off -->
    <function model="hr.work.entry.type" name="write">
        <value model="hr.work.entry.type" eval="obj().sudo().search([('code', '=', 'LEAVE120')]).ids"/>
        <value eval="{
            'requires_allocation': True,
            'employee_requests': False,
            'leave_validation_type': 'both',
            'allocation_validation_type': 'hr',
            'request_unit': 'day',
            'unit_of_measure': 'day',
            'leave_notif_subtype_id': ref('hr_holidays.mt_leave'),
            'allocation_notif_subtype_id': ref('hr_holidays.mt_leave_allocation'),
            'color': 2,
            'sequence': 1,
        }"/>
    </function>

    <!-- Sick Time Off -->
    <function model="hr.work.entry.type" name="write">
        <value model="hr.work.entry.type" eval="obj().sudo().search([('code', '=', 'LEAVE110')]).ids"/>
        <value eval="{
            'requires_allocation': False,
            'request_unit': 'day',
            'unit_of_measure': 'day',
            'leave_notif_subtype_id': ref('hr_holidays.mt_leave_sick'),
            'support_document': True,
            'hide_on_dashboard': True,
            'color': 3,
            'sequence': 2,
        }"/>
    </function>

    <!-- Compensatory -->
    <function model="hr.work.entry.type" name="write">
        <value model="hr.work.entry.type" eval="obj().sudo().search([('code', '=', 'LEAVE105')]).ids"/>
        <value eval="{
            'requires_allocation': True,
            'employee_requests': True,
            'leave_validation_type': 'manager',
            'allocation_validation_type': 'hr',
            'request_unit': 'day',
            'unit_of_measure': 'day',
            'leave_notif_subtype_id': ref('hr_holidays.mt_leave'),
            'color': 4,
            'sequence': 4,
        }"/>
    </function>

    <!-- Unpaid -->
    <function model="hr.work.entry.type" name="write">
        <value model="hr.work.entry.type" eval="obj().sudo().search([('code', '=', 'LEAVE90')]).ids"/>
        <value eval="{
            'requires_allocation': False,
            'leave_validation_type': 'both',
            'allocation_validation_type': 'hr',
            'request_unit': 'hour',
            'unit_of_measure': 'hour',
            'hide_on_dashboard': True,
            'unpaid': True,
            'leave_notif_subtype_id': ref('hr_holidays.mt_leave_unpaid'),
            'color': 5,
            'sequence': 3,
        }"/>
    </function>

