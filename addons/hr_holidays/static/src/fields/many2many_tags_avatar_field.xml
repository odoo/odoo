<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
    <t t-inherit="web.Many2ManyTagsAvatarField.option" t-inherit-mode="extension">
        <xpath expr="//span[@t-out='autoCompleteItemScope.label']" position="after">
            <span t-if="autoCompleteItemScope.record.leave_date_to" class="text-warning ms-1 small">
                <i class="fa fa-plane" role="img"/> <t t-esc="getOutOfOfficeDateEndText(autoCompleteItemScope.record.leave_date_to)"/>
            </span>
        </xpath>
    </t>
</templates>
