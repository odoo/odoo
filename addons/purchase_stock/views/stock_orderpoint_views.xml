<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record model="ir.ui.view" id="warehouse_orderpoint_search_inherit">
        <field name="name">stock.warehouse.orderpoint.search.inherit</field>
        <field name="model">stock.warehouse.orderpoint</field>
        <field name="inherit_id" ref="stock.stock_reorder_report_search"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='product_category_id']" position="after">
                <field string="Vendor" name="effective_vendor_id"/>
                <field name="available_vendor"/>
            </xpath>
        </field>
    </record>
</odoo>
