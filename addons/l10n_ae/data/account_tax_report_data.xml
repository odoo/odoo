                                        <field name="engine">tax_tags</field>
                                        <field name="formula">a. Abu Dhabi (Base)</field>
                                    </record>
                                </field>
                            </record>
                            <record id="tax_report_line_standard_rated_supplies_base_dubai" model="account.report.line">
