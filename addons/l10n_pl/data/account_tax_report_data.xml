<?xml version="1.0" encoding="utf-8"?>
<odoo auto_sequence="1">
    <record id="tax_report" model="account.report">
        <field name="name">Tax Report</field>
        <field name="name@pl">Raporty Podatkowe</field>
        <field name="root_report_id" ref="account.generic_tax_report"/>
        <field name="country_id" ref="base.pl"/>
        <field name="availability_condition">country</field>
        <field name="allow_foreign_vat" eval="True"/>
        <field name="use_sections" eval="True"/>
        <field name="section_report_ids" eval="[Command.set([ref('l10n_pl.tax_report_data'),
                                                            ref('l10n_pl.tax_return_submission_details')])]"/>
    </record>
</odoo>