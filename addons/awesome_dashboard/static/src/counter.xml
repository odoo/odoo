<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">

    <t t-name="awesome_dashboard.Counter">
        <span class="m-1 p-2 border">
            <button class="btn btn-primary" t-on-click="decrement">-</button>
            <span class="mx-2" t-esc="state.value"/>
            <button class="btn btn-primary" t-on-click="increment">+</button>
        </span>
    </t>

</templates>
