                            </record>
                        </field>
                    </record>
                    <record id="tax_report_line_exempt_purchases_base" model="account.report.line">
                        <field name="name">11. Exempt Purchases (Base)</field>
                        <field name="code">SA_EXM_PUR_B</field>
