        <field name="partner_id" ref="base.partner_demo_company_hu"/>
    </record>

    <record id="base.demo_bank_hu" model="res.partner.bank" forcecreate="1">
        <field name="acc_number">HU55107000247733442327874189</field>
        <field name="partner_id" ref="base.partner_demo_company_hu"/>
