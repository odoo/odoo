<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data auto_sequence="1">

        <record id="400v5_tax_report" model="account.report">
            <field name="name">Form 400v5: IT</field>
            <field name="root_report_id" ref="account.generic_tax_report"/>
            <field name="country_id" ref="base.bo"/>
            <field name="filter_fiscal_position" eval="True"/>
            <field name="availability_condition">country</field>
            <field name="column_ids">
                <record id="400v5_tax_report_code" model="account.report.column">
                    <field name="name">Box Code</field>
                    <field name="expression_label">code</field>
                    <field name="figure_type">string</field>
                </record>
                <record id="400v5_tax_report_balance" model="account.report.column">
                    <field name="name">Amount</field>
                    <field name="expression_label">balance</field>
                    <field name="figure_type">monetary</field>
                </record>
            </field>
            <field name="line_ids">
                <record id="f400_v5_tax_report" model="account.report.line">
                    <field name="sequence" eval="False"/>
                    <field name="name">Form 400v5: IT Transactions Tax</field>
                    <field name="code">f400_v5_tax_report</field>
                    <field name="hierarchy_level" eval="False"/>
                    <field name="children_ids">
                        <record id="it_base_determination_report" model="account.report.line">
                            <field name="sequence" eval="100"/>
                            <field name="name">Determination of the Taxable Base and Tax</field>
                            <field name="code">it_base_determination_report</field>
                            <field name="children_ids">
                                <record id="it_total_income_tax_line" model="account.report.line">
                                    <field name="sequence" eval="101"/>
                                    <field name="name">Total gross income accrued and/or received in kind (includes exempt income)</field>
                                    <field name="code">it13</field>
                                    <field name="hide_if_zero" eval="False"/>
                                    <field name="expression_ids">
                                        <record id="it_total_income_tax_code" model="account.report.expression">
                                            <field name="label">code</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">13</field>
                                        </record>
                                        <record id="it_total_income_tax_tag" model="account.report.expression">
                                            <field name="label">balance</field>
                                            <field name="engine">tax_tags</field>
                                            <field name="formula">it13</field>
                                        </record>
                                    </field>
                                </record>
                                <record id="it_exempt_tax_line" model="account.report.line">
                                    <field name="sequence" eval="102"/>
                                    <field name="name">Exempt income</field>
                                    <field name="code">it32</field>
                                    <field name="hide_if_zero" eval="False"/>
                                    <field name="expression_ids">
                                        <record id="it_exempt_tax_code" model="account.report.expression">
                                            <field name="label">code</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">32</field>
                                        </record>
                                        <record id="it_exempt_tax_tag" model="account.report.expression">
                                            <field name="label">balance</field>
                                            <field name="engine">tax_tags</field>
                                            <field name="formula">it32</field>
                                        </record>
                                    </field>
                                </record>
                                <record id="it_base_tax_line" model="account.report.line">
                                    <field name="sequence" eval="103"/>
                                    <field name="name">Taxable base (C13 - C32)</field>
                                    <field name="code">it24</field>
                                    <field name="hide_if_zero" eval="False"/>
                                    <field name="expression_ids">
                                        <record id="it_base_tax_code" model="account.report.expression">
                                            <field name="label">code</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">24</field>
                                        </record>
                                        <record id="it_base_tax_balance" model="account.report.expression">
                                            <field name="label">balance</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">it13.balance - it32.balance</field>
                                        </record>
                                    </field>
                                </record>
                                <record id="it_determined_amount_tax_line" model="account.report.line">
                                    <field name="sequence" eval="104"/>
                                    <field name="name">Determined Tax (C24 * 3%)</field>
                                    <field name="code">it909</field>
                                    <field name="hide_if_zero" eval="False"/>
                                    <field name="expression_ids">
                                        <record id="it_determined_amount_tax_code" model="account.report.expression">
                                            <field name="label">code</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">909</field>
                                        </record>
                                        <record id="it_determined_amount_tax_tag" model="account.report.expression">
                                            <field name="label">balance</field>
                                            <field name="engine">tax_tags</field>
                                            <field name="formula">it909</field>
                                        </record>
                                    </field>
                                </record>
                            </field>
                        </record>
                        <record id="it_balance_determination_report" model="account.report.line">
                            <field name="sequence" eval="200"/>
                            <field name="name">Determination of the Balance</field>
                            <field name="code">it_balance_determination_report</field>
                            <field name="children_ids">
                                <record id="paid_iue_to_compensate_tax_line" model="account.report.line">
                                    <field name="sequence" eval="201"/>
                                    <field name="name">IUE paid to be offset (1st Instance or "IUE Balance to be offset" from Form 400, C619 from the previous period)</field>
                                    <field name="code">it664</field>
                                    <field name="hide_if_zero" eval="False"/>
                                    <field name="expression_ids">
                                        <record id="paid_iue_to_compensate_tax_code" model="account.report.expression">
                                            <field name="label">code</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">664</field>
                                        </record>
                                        <record id="paid_iue_to_compensate_tax_balance" model="account.report.expression">
                                            <field name="label">balance</field>
                                            <field name="engine">external</field>
                                            <field name="formula">sum</field>
                                            <field name="subformula">editable</field>
                                        </record>
                                    </field>
                                </record>
                                <record id="it_determined_treasury_favor_balance_tax_line" model="account.report.line">
                                    <field name="sequence" eval="202"/>
                                    <field name="name">Determined Tax Balance in favor of the Treasury (C909 - C664; If > 0)</field>
                                    <field name="code">it1001</field>
                                    <field name="hide_if_zero" eval="False"/>
                                    <field name="expression_ids">
                                        <record id="it_determined_treasury_favor_balance_tax_code" model="account.report.expression">
                                            <field name="label">code</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">1001</field>
                                        </record>
                                        <record id="it_determined_treasury_favor_balance_tax_balance" model="account.report.expression">
                                            <field name="label">balance</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">it909.balance - it664.balance</field>
                                            <field name="subformula">if_above(CUR(0))</field>
                                        </record>
                                    </field>
                                </record>
                                <record id="it_payment_on_account_in_sworn_statements_tax_line" model="account.report.line">
                                    <field name="sequence" eval="203"/>
                                    <field name="name">Payments on account made in Sworn Statements and/or Payment Slips corresponding to the declared period</field>
                                    <field name="code">it622</field>
                                    <field name="hide_if_zero" eval="False"/>
                                    <field name="expression_ids">
                                        <record id="it_payment_on_account_in_sworn_statements_tax_code" model="account.report.expression">
                                            <field name="label">code</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">622</field>
                                        </record>
                                        <record id="it_payment_on_account_in_sworn_statements_tax_balance" model="account.report.expression">
                                            <field name="label">balance</field>
                                            <field name="engine">external</field>
                                            <field name="formula">sum</field>
                                            <field name="subformula">editable</field>
                                        </record>
                                    </field>
                                </record>
                                <record id="it_previous_payments_on_account_to_compensate_tax_line" model="account.report.line">
                                    <field name="sequence" eval="204"/>
                                    <field name="name">Balance of Payments on account from the previous period to be offset (C747 from Form 400 of the previous period)</field>
                                    <field name="code">it640</field>
                                    <field name="hide_if_zero" eval="False"/>
                                    <field name="expression_ids">
                                        <record id="it_previous_payments_on_account_to_compensate_tax_code" model="account.report.expression">
                                            <field name="label">code</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">640</field>
                                        </record>
                                        <record id="it_previous_payments_on_account_to_compensate_tax_balance" model="account.report.expression">
                                            <field name="label">balance</field>
                                            <field name="engine">external</field>
                                            <field name="formula">sum</field>
                                            <field name="subformula">editable</field>
                                        </record>
                                    </field>
                                </record>
                                <record id="it_taxpayer_favor_balance_payments_on_account_tax_line" model="account.report.line">
                                    <field name="sequence" eval="205"/>
                                    <field name="name">Balance for Payments on account in favor of the Taxpayer (C622+C640-C1001; If > 0)</field>
                                    <field name="code">it643</field>
                                    <field name="hide_if_zero" eval="False"/>
                                    <field name="expression_ids">
                                        <record id="it_taxpayer_favor_balance_payments_on_account_tax_code" model="account.report.expression">
                                            <field name="label">code</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">643</field>
                                        </record>
                                        <record id="it_taxpayer_favor_balance_payments_on_account_tax_balance" model="account.report.expression">
                                            <field name="label">balance</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">it622.balance + it640.balance - it1001.balance</field>
                                            <field name="subformula">if_above(CUR(0))</field>
                                        </record>
                                    </field>
                                </record>
                                <record id="it_treasury_favor_balance_tax_line" model="account.report.line">
                                    <field name="sequence" eval="206"/>
                                    <field name="name">Balance in favor of the Treasury (C1001-C622-C640; If > 0)</field>
                                    <field name="code">it996</field>
                                    <field name="hide_if_zero" eval="False"/>
                                    <field name="expression_ids">
                                        <record id="it_treasury_favor_balance_tax_code" model="account.report.expression">
                                            <field name="label">code</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">996</field>
                                        </record>
                                        <record id="it_treasury_favor_balance_tax_balance" model="account.report.expression">
                                            <field name="label">balance</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">it1001.balance - it622.balance - it640.balance</field>
                                            <field name="subformula">if_above(CUR(0))</field>
                                        </record>
                                    </field>
                                </record>
                            </field>
                        </record>
                        <record id="it_debt_determination_report" model="account.report.line">
                            <field name="sequence" eval="300"/>
                            <field name="name">Determination of the Tax Debt</field>
                            <field name="code">it_debt_determination_report</field>
                            <field name="children_ids">
