                                            <field name="label">balance</field>
                                            <field name="engine">external</field>
                                            <field name="formula">sum</field>
                                            <field name="subformula">editable</field>
                                        </record>
                                    </field>
                                </record>
                                <record id="it_late_submission_idf_finetax_line" model="account.report.line">
                                    <field name="sequence" eval="305"/>
                                    <field name="name">IDF Fine for Increase in Determined Tax in Rectificatory Sworn Statements submitted late</field>
                                    <field name="code">it967</field>
                                    <field name="hide_if_zero" eval="False"/>
                                    <field name="expression_ids">
                                        <record id="it_late_submission_idf_finetax_code" model="account.report.expression">
                                            <field name="label">code</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">967</field>
                                        </record>
                                        <record id="it_late_submission_idf_finetax_balance" model="account.report.expression">
                                            <field name="label">balance</field>
                                            <field name="engine">external</field>
                                            <field name="formula">sum</field>
                                            <field name="subformula">editable</field>
                                        </record>
                                    </field>
                                </record>
                                <record id="it_total_debt_tax_line" model="account.report.line">
                                    <field name="sequence" eval="306"/>
                                    <field name="name">Total Tax Debt (C924+C925+C938+C954+C967)</field>
                                    <field name="code">it955</field>
                                    <field name="hide_if_zero" eval="False"/>
                                    <field name="expression_ids">
                                        <record id="it_total_debt_tax_code" model="account.report.expression">
                                            <field name="label">code</field>
                                            <field name="auditable" eval="False"/>
