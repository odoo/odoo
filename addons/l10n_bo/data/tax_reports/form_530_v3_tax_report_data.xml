<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data auto_sequence="1">

        <record id="530v3_tax_report" model="account.report">
            <field name="name">Form 530v3: IUE BE Withholdings</field>
            <field name="root_report_id" ref="account.generic_tax_report"/>
            <field name="country_id" ref="base.bo"/>
            <field name="filter_fiscal_position" eval="True"/>
            <field name="availability_condition">country</field>
            <field name="column_ids">
                <record id="530v3_tax_report_code" model="account.report.column">
                    <field name="name">Box Code</field>
                    <field name="expression_label">code</field>
                    <field name="figure_type">string</field>
                </record>
                <record id="530v3_tax_report_balance" model="account.report.column">
                    <field name="name">Amount</field>
                    <field name="expression_label">balance</field>
                    <field name="figure_type">monetary</field>
                </record>
            </field>
            <field name="line_ids">
                <record id="f530_v3_tax_report" model="account.report.line">
                    <field name="sequence" eval="False"/>
                    <field name="name">Form 530v3: IUE Withholding for Foreign Beneficiaries</field>
                    <field name="code">f530_v3_tax_report</field>
                    <field name="hierarchy_level" eval="False"/>
                    <field name="children_ids">
                        <record id="iue_be_retention_report" model="account.report.line">
                            <field name="sequence" eval="100"/>
                            <field name="name">Withholdings for Foreign Beneficiaries</field>
                            <field name="code">iue_be_retention_report</field>
                            <field name="children_ids">
                                <record id="iue_be_retention_remitted_amount_tax_line" model="account.report.line">
                                    <field name="sequence" eval="101"/>
                                    <field name="name">Remitted Amount (Art. 51 Law No. 843)</field>
                                    <field name="code">iueBe13</field>
                                    <field name="hide_if_zero" eval="False"/>
                                    <field name="expression_ids">
                                        <record id="iue_be_retention_remitted_amount_tax_code" model="account.report.expression">
                                            <field name="label">code</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">13</field>
                                        </record>
                                        <record id="iue_be_retention_remitted_amount_tax_tag" model="account.report.expression">
                                            <field name="label">balance</field>
                                            <field name="engine">tax_tags</field>
                                            <field name="formula">iueBe13</field>
                                        </record>
                                    </field>
                                </record>
                                <record id="iue_be_retention_presumed_net_profit_tax_line" model="account.report.line">
                                    <field name="sequence" eval="102"/>
                                    <field name="name">Presumed Taxable Net Profit (C013 * 50%)</field>
                                    <field name="code">iueBe26</field>
                                    <field name="hide_if_zero" eval="False"/>
                                    <field name="expression_ids">
                                        <record id="iue_be_retention_presumed_net_profit_tax_code" model="account.report.expression">
                                            <field name="label">code</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">26</field>
                                        </record>
                                        <record id="iue_be_retention_presumed_net_profit_tax_tag" model="account.report.expression">
                                            <field name="label">balance</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">iueBe13.balance * 0.5</field>
                                        </record>
                                    </field>
                                </record>
                            </field>
                        </record>
                        <record id="iue_be_tax_amount_determination_report" model="account.report.line">
                            <field name="sequence" eval="200"/>
                            <field name="name">Determination of the Tax</field>
                            <field name="code">iue_be_tax_amount_determination_report</field>
                            <field name="children_ids">
                                <record id="determined_iue_be_retention_amount_tax_line" model="account.report.line">
                                    <field name="sequence" eval="201"/>
                                    <field name="name">Determined Tax (C26*25%)</field>
                                    <field name="code">iueBe909</field>
                                    <field name="hide_if_zero" eval="False"/>
                                    <field name="expression_ids">
                                        <record id="determined_iue_be_retention_amount_tax_code" model="account.report.expression">
                                            <field name="label">code</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">909</field>
                                        </record>
                                        <record id="determined_iue_be_retention_amount_tax_tag" model="account.report.expression">
                                            <field name="label">balance</field>
                                            <field name="engine">tax_tags</field>
                                            <field name="formula">iueBe909</field>
                                        </record>
                                    </field>
                                </record>
                            </field>
                        </record>

                        <record id="iue_be_balance_determination_report" model="account.report.line">
                            <field name="sequence" eval="300"/>
                            <field name="name">Determination of Balances</field>
                            <field name="code">iue_be_balance_determination_report</field>
                            <field name="children_ids">
                                <record id="iue_be_retention_payment_on_account_in_sworn_statements_tax_line" model="account.report.line">
                                    <field name="sequence" eval="301"/>
                                    <field name="name">Payments on Account Made in Previous Sworn Statements and/or Payment Slips for the Fiscal Period Being Settled (BP 3050 V.3 C909)</field>
                                    <field name="code">iueBe622</field>
                                    <field name="hide_if_zero" eval="False"/>
                                    <field name="expression_ids">
                                        <record id="iue_be_retention_payment_on_account_in_sworn_statements_tax_code" model="account.report.expression">
                                            <field name="label">code</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">622</field>
                                        </record>
                                        <record id="iue_be_retention_payment_on_account_in_sworn_statements_tax_tag" model="account.report.expression">
                                            <field name="label">balance</field>
                                            <field name="engine">external</field>
                                            <field name="formula">sum</field>
                                            <field name="subformula">editable</field>
                                        </record>
                                    </field>
                                </record>
                                <record id="previous_iue_be_retention_payments_on_account_to_compensate_tax_line" model="account.report.line">
                                    <field name="sequence" eval="302"/>
                                    <field name="name">Available Balance of Payments from the Previous Period to be Offset</field>
                                    <field name="code">iueBe640</field>
                                    <field name="hide_if_zero" eval="False"/>
                                    <field name="expression_ids">
                                        <record id="previous_iue_be_retention_payments_on_account_to_compensate_tax_code" model="account.report.expression">
                                            <field name="label">code</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">640</field>
                                        </record>
                                        <record id="previous_iue_be_retention_payments_on_account_to_compensate_tax_tag" model="account.report.expression">
                                            <field name="label">balance</field>
                                            <field name="engine">external</field>
                                            <field name="formula">sum</field>
                                            <field name="subformula">editable</field>
                                        </record>
                                    </field>
                                </record>
                                <record id="iue_be_retention_taxpayer_favor_balance_payments_on_account_tax_line" model="account.report.line">
                                    <field name="sequence" eval="303"/>
                                    <field name="name">Balance in Favor of the Taxpayer for the Next Period (C622+C640-C909; If &gt; 0)</field>
                                    <field name="code">iueBe747</field>
                                    <field name="hide_if_zero" eval="False"/>
                                    <field name="expression_ids">
                                        <record id="iue_be_retention_taxpayer_favor_balance_payments_on_account_tax_code" model="account.report.expression">
                                            <field name="label">code</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">747</field>
                                        </record>
                                        <record id="iue_be_retention_taxpayer_favor_balance_payments_on_account_tax_balance" model="account.report.expression">
                                            <field name="label">balance</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">iueBe622.balance + iueBe640.balance - iueBe909.balance</field>
                                            <field name="subformula">if_above(CUR(0))</field>
                                        </record>
                                    </field>
                                </record>
                                <record id="iue_be_retention_treasury_favor_balance_tax_line" model="account.report.line">
                                    <field name="sequence" eval="304"/>
                                    <field name="name">Definitive Balance in Favor of the Treasury (C909 - C622 - C640; If &gt; 0)</field>
                                    <field name="code">iueBe996</field>
                                    <field name="hide_if_zero" eval="False"/>
                                    <field name="expression_ids">
                                        <record id="iue_be_retention_treasury_favor_balance_tax_code" model="account.report.expression">
                                            <field name="label">code</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">996</field>
                                        </record>
                                        <record id="iue_be_retention_treasury_favor_balance_tax_balance" model="account.report.expression">
                                            <field name="label">balance</field>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">iueBe909.balance - iueBe622.balance - iueBe640.balance</field>
                                            <field name="subformula">if_above(CUR(0))</field>
                                        </record>
                                    </field>
                                </record>
                            </field>
                        </record>

                        <record id="iue_be_fine_determination_report" model="account.report.line">
                            <field name="sequence" eval="400"/>
                            <field name="name">Determination of Tax Debt and Fines</field>
                            <field name="code">iue_be_fine_determination_report</field>
                            <field name="children_ids">
                                <record id="iue_be_retention_payment_in_values_tax_line" model="account.report.line">
                                    <field name="sequence" eval="401"/>
                                    <field name="name">Assignment of Credit in Securities (Subject to Verification and Confirmation by SIN)</field>
                                    <field name="code">iueBe677</field>
                                    <field name="hide_if_zero" eval="False"/>
                                    <field name="expression_ids">
                                        <record id="iue_be_retention_payment_in_values_tax_code" model="account.report.expression">
                                            <field name="label">code</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">677</field>
                                        </record>
                                        <record id="iue_be_retention_payment_in_values_tax_tag" model="account.report.expression">
                                            <field name="label">balance</field>
                                            <field name="engine">external</field>
                                            <field name="formula">sum</field>
                                            <field name="subformula">editable</field>
                                        </record>
                                    </field>
                                </record>
                                <record id="iue_be_retention_cash_payment_tax_line" model="account.report.line">
                                    <field name="sequence" eval="402"/>
                                    <field name="name">Cash Payment Tax (C996-C677; If &gt; 0)</field>
                                    <field name="code">iueBe576</field>
                                    <field name="hide_if_zero" eval="False"/>
                                    <field name="expression_ids">
                                        <record id="iue_be_retention_cash_payment_tax_code" model="account.report.expression">
                                            <field name="label">code</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">576</field>
                                        </record>
                                        <record id="iue_be_retention_cash_payment_tax_balance" model="account.report.expression">
                                            <field name="label">balance</field>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">iueBe996.balance - iueBe677.balance</field>
                                            <field name="subformula">if_above(CUR(0))</field>
                                        </record>
                                    </field>
                                </record>
                            </field>
                        </record>
                    </field>
                </record>
            </field>
        </record>

    </data>
</odoo>