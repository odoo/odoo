<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data auto_sequence="1">

        <record id="551v3_tax_report" model="account.report">
            <field name="name">Form 551v3: Partial IUE BE Withholding</field>
            <field name="root_report_id" ref="account.generic_tax_report"/>
            <field name="country_id" ref="base.bo"/>
            <field name="filter_fiscal_position" eval="True"/>
            <field name="availability_condition">country</field>
            <field name="column_ids">
                <record id="551v3_tax_report_code" model="account.report.column">
                    <field name="name">Box Code</field>
                    <field name="expression_label">code</field>
                    <field name="figure_type">string</field>
                </record>
                <record id="551v3_tax_report_balance" model="account.report.column">
                    <field name="name">Amount</field>
                    <field name="expression_label">balance</field>
                    <field name="figure_type">monetary</field>
                </record>
            </field>
            <field name="line_ids">
                <record id="f551_v3_tax_report" model="account.report.line">
                    <field name="sequence" eval="False"/>
                    <field name="name">Form 551v3: IUE Withholding for Foreign Beneficiaries â€“ Partial Activity</field>
                    <field name="code">f551_v3_tax_report</field>
                    <field name="hierarchy_level" eval="False"/>
                    <field name="children_ids">
                        <record id="iue_be_partial_retention_report" model="account.report.line">
                            <field name="sequence" eval="100"/>
                            <field name="name">Withholdings by Domestic Companies Contracting with Foreign Companies for Partially Performed Activities</field>
                            <field name="code">iue_be_partial_retention_report</field>
                            <field name="children_ids">
                                <record id="iue_be_partial_retention_remitted_amount_line" model="account.report.line">
                                    <field name="sequence" eval="101"/>
                                    <field name="name">Remitted Amount</field>
                                    <field name="code">iueBep13</field>
                                    <field name="hide_if_zero" eval="False"/>
                                    <field name="expression_ids">
                                        <record id="iue_be_partial_retention_remitted_amount_tax_code" model="account.report.expression">
                                            <field name="label">code</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">13</field>
                                        </record>
                                        <record id="iue_be_partial_retention_remitted_amount_tax_tag" model="account.report.expression">
                                            <field name="label">balance</field>
                                            <field name="engine">tax_tags</field>
                                            <field name="formula">iueBep13</field>
                                        </record>
                                    </field>
                                </record>
                                <record id="iue_be_partial_retention_presumed_taxable_net_income_line" model="account.report.line">
                                    <field name="sequence" eval="102"/>
                                    <field name="name">Presumed Taxable Net Income (C103 * 20%)</field>
                                    <field name="code">iueBep26</field>
                                    <field name="hide_if_zero" eval="False"/>
                                    <field name="expression_ids">
                                        <record id="iue_be_partial_retention_presumed_taxable_net_income_code" model="account.report.expression">
                                            <field name="label">code</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">26</field>
                                        </record>
                                        <record id="iue_be_partial_retention_presumed_taxable_net_income_tag" model="account.report.expression">
                                            <field name="label">balance</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">iueBep13.balance * 0.2</field>
                                        </record>
                                    </field>
                                </record>
                                <record id="iue_be_partial_retention_presumptive_income_line" model="account.report.line">
                                    <field name="sequence" eval="103"/>
                                    <field name="name">Presumed Income (C26 * 50%)</field>
                                    <field name="code">iueBep27</field>
                                    <field name="hide_if_zero" eval="False"/>
                                    <field name="expression_ids">
                                        <record id="iue_be_partial_retention_presumptive_income_code" model="account.report.expression">
                                            <field name="label">code</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">27</field>
                                        </record>
                                        <record id="iue_be_partial_retention_presumptive_income_tag" model="account.report.expression">
                                            <field name="label">balance</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">iueBep26.balance * 0.5</field>
                                        </record>
                                    </field>
                                </record>
                            </field>
                        </record>

                        <record id="iue_be_partial_retention_tax_amount_determination_report" model="account.report.line">
                            <field name="sequence" eval="200"/>
                            <field name="name">Determination of the Tax</field>
                            <field name="code">iue_be_partial_retention_tax_amount_determination_report</field>
                            <field name="children_ids">
                                <record id="determined_iue_be_partial_retention_amount_tax_line" model="account.report.line">
                                    <field name="sequence" eval="201"/>
                                    <field name="name">Determined Tax (25% * (C27))</field>
                                    <field name="code">iueBep909</field>
                                    <field name="hide_if_zero" eval="False"/>
                                    <field name="expression_ids">
                                        <record id="determined_iue_be_partial_retention_amount_tax_code" model="account.report.expression">
                                            <field name="label">code</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">909</field>
                                        </record>
                                        <record id="determined_iue_be_partial_retention_amount_tax_tag" model="account.report.expression">
                                            <field name="label">balance</field>
                                            <field name="engine">tax_tags</field>
                                            <field name="formula">iueBep909</field>
                                        </record>
                                    </field>
                                </record>
                            </field>
                        </record>

                        <record id="iue_be_partial_retention_balance_determination_report" model="account.report.line">
                            <field name="sequence" eval="300"/>
                            <field name="name">Determination of Balances</field>
                            <field name="code">iue_be_partial_retention_balance_determination_report</field>
                            <field name="children_ids">
                                <record id="iue_be_partial_retention_payment_on_account_in_sworn_statements_tax_line" model="account.report.line">
                                    <field name="sequence" eval="301"/>
                                    <field name="name">Payments on Account Made in Previous Sworn Statements and/or Payment Slips for the Fiscal Period Being Settled (BP 3050 V.3 C909)</field>
                                    <field name="code">iueBep622</field>
                                    <field name="hide_if_zero" eval="False"/>
                                    <field name="expression_ids">
                                        <record id="iue_be_partial_retention_payment_on_account_in_sworn_statements_tax_code" model="account.report.expression">
                                            <field name="label">code</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">622</field>
                                        </record>
                                        <record id="iue_be_partial_retention_payment_on_account_in_sworn_statements_tax_tag" model="account.report.expression">
                                            <field name="label">balance</field>
                                            <field name="engine">external</field>
                                            <field name="formula">sum</field>
                                            <field name="subformula">editable</field>
                                        </record>
                                    </field>
                                </record>
                                <record id="previous_iue_be_partial_retention_payments_on_account_to_compensate_tax_line" model="account.report.line">
                                    <field name="sequence" eval="302"/>
                                    <field name="name">Available Balance of Payments from the Previous Period to be Offset</field>
                                    <field name="code">iueBep640</field>
                                    <field name="hide_if_zero" eval="False"/>
                                    <field name="expression_ids">
                                        <record id="previous_iue_be_partial_retention_payments_on_account_to_compensate_tax_code" model="account.report.expression">
                                            <field name="label">code</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">640</field>
                                        </record>
                                        <record id="previous_iue_be_partial_retention_payments_on_account_to_compensate_tax_tag" model="account.report.expression">
                                            <field name="label">balance</field>
                                            <field name="engine">external</field>
                                            <field name="formula">sum</field>
                                            <field name="subformula">editable</field>
                                        </record>
                                    </field>
                                </record>
                                <record id="iue_be_partial_retention_taxpayer_favor_balance_payments_on_account_tax_line" model="account.report.line">
                                    <field name="sequence" eval="303"/>
                                    <field name="name">Balance in Favor of the Taxpayer for the Next Period (C622+C640-C909; If &gt; 0)</field>
                                    <field name="code">iueBep747</field>
                                    <field name="hide_if_zero" eval="False"/>
                                    <field name="expression_ids">
                                        <record id="iue_be_partial_retention_taxpayer_favor_balance_payments_on_account_tax_code" model="account.report.expression">
                                            <field name="label">code</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">747</field>
                                        </record>
                                        <record id="iue_be_partial_retention_taxpayer_favor_balance_payments_on_account_tax_balance" model="account.report.expression">
                                            <field name="label">balance</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">iueBep622.balance + iueBep640.balance - iueBep909.balance</field>
                                            <field name="subformula">if_above(CUR(0))</field>
                                        </record>
                                    </field>
                                </record>
                                <record id="iue_be_partial_retention_treasury_favor_balance_tax_line" model="account.report.line">
                                    <field name="sequence" eval="304"/>
                                    <field name="name">Definitive Balance in Favor of the Treasury (C909 - C622 - C640; If &gt; 0)</field>
                                    <field name="code">iueBep996</field>
                                    <field name="hide_if_zero" eval="False"/>
                                    <field name="expression_ids">
                                        <record id="iue_be_partial_retention_treasury_favor_balance_tax_code" model="account.report.expression">
                                            <field name="label">code</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">996</field>
                                        </record>
                                        <record id="iue_be_partial_retention_treasury_favor_balance_tax_balance" model="account.report.expression">
                                            <field name="label">balance</field>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">iueBep909.balance - iueBep622.balance - iueBep640.balance</field>
                                            <field name="subformula">if_above(CUR(0))</field>
                                        </record>
                                    </field>
                                </record>
                            </field>
                        </record>

                        <record id="iue_be_partial_retention_fine_determination_report" model="account.report.line">
                            <field name="sequence" eval="400"/>
                            <field name="name">Determination of Tax Debt and Fines</field>
                            <field name="code">iue_be_partial_retention_fine_determination_report</field>
                            <field name="children_ids">
                                <record id="iue_be_partial_retention_omitted_tribute_tax_line" model="account.report.line">
                                    <field name="sequence" eval="401"/>
                                    <field name="name">Omitted Tax (C996)</field>
                                    <field name="code">iueBep924</field>
                                    <field name="hide_if_zero" eval="False"/>
                                    <field name="expression_ids">
                                        <record id="iue_be_partial_retention_omitted_tribute_tax_code" model="account.report.expression">
                                            <field name="label">code</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">924</field>
                                        </record>
                                        <record id="iue_be_partial_retention_omitted_tribute_tax_balance" model="account.report.expression">
                                            <field name="label">balance</field>
                                            <field name="engine">external</field>
                                            <field name="formula">sum</field>
                                            <field name="subformula">editable</field>
                                        </record>
                                    </field>
                                </record>
                                <record id="updated_iue_be_partial_retention_omitted_tribute_tax_line" model="account.report.line">
                                    <field name="sequence" eval="402"/>
                                    <field name="name">Updated Value on Omitted Tax</field>
                                    <field name="code">iueBep925</field>
                                    <field name="hide_if_zero" eval="False"/>
                                    <field name="expression_ids">
                                        <record id="updated_iue_be_partial_retention_omitted_tribute_tax_code" model="account.report.expression">
                                            <field name="label">code</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">925</field>
                                        </record>
                                        <record id="updated_iue_be_partial_retention_omitted_tribute_tax_balance" model="account.report.expression">
                                            <field name="label">balance</field>
                                            <field name="engine">external</field>
                                            <field name="formula">sum</field>
                                            <field name="subformula">editable</field>
                                        </record>
                                    </field>
                                </record>
                                <record id="interest_on_updated_iue_be_partial_retention_omitted_tribute_tax_line" model="account.report.line">
                                    <field name="sequence" eval="403"/>
                                    <field name="name">Interest on Updated Omitted Tax</field>
                                    <field name="code">iueBep938</field>
                                    <field name="hide_if_zero" eval="False"/>
                                    <field name="expression_ids">
                                        <record id="interest_on_updated_iue_be_partial_retention_omitted_tribute_tax_code" model="account.report.expression">
                                            <field name="label">code</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">938</field>
                                        </record>
                                        <record id="interest_on_updated_iue_be_partial_retention_omitted_tribute_tax_balance" model="account.report.expression">
                                            <field name="label">balance</field>
                                            <field name="engine">external</field>
                                            <field name="formula">sum</field>
                                            <field name="subformula">editable</field>
                                        </record>
                                    </field>
                                </record>
                                <record id="late_iue_be_partial_retention_submission_fine_tax_line" model="account.report.line">
                                    <field name="sequence" eval="404"/>
                                    <field name="name">Fine for Non-compliance with Formal Duties (IDF) for Late Submission</field>
                                    <field name="code">iueBep954</field>
                                    <field name="hide_if_zero" eval="False"/>
                                    <field name="expression_ids">
                                        <record id="late_iue_be_partial_retention_submission_fine_tax_code" model="account.report.expression">
                                            <field name="label">code</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">954</field>
                                        </record>
                                        <record id="late_iue_be_partial_retention_submission_fine_tax_balance" model="account.report.expression">
                                            <field name="label">balance</field>
                                            <field name="engine">external</field>
                                            <field name="formula">sum</field>
                                            <field name="subformula">editable</field>
                                        </record>
                                    </field>
                                </record>
                                <record id="iue_be_partial_retention_payment_in_values_tax_line" model="account.report.line">
                                    <field name="sequence" eval="405"/>
                                    <field name="name">Payment with Fiscal Securities (Subject to Verification and Confirmation by SIN)</field>
                                    <field name="code">iueBep677</field>
                                    <field name="hide_if_zero" eval="False"/>
                                    <field name="expression_ids">
                                        <record id="iue_be_partial_retention_payment_in_values_tax_code" model="account.report.expression">
                                            <field name="label">code</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">677</field>
                                        </record>
                                        <record id="iue_be_partial_retention_payment_in_values_tax_tag" model="account.report.expression">
                                            <field name="label">balance</field>
                                            <field name="engine">external</field>
                                            <field name="formula">sum</field>
                                            <field name="subformula">editable</field>
                                        </record>
                                    </field>
                                </record>
                                <record id="iue_be_partial_retention_cash_payment_tax_line" model="account.report.line">
                                    <field name="sequence" eval="406"/>
                                    <field name="name">Cash Payment Tax (C996 - C677; If &gt; 0)</field>
                                    <field name="code">iueBep576</field>
                                    <field name="hide_if_zero" eval="False"/>
                                    <field name="expression_ids">
                                        <record id="iue_be_partial_retention_cash_payment_tax_code" model="account.report.expression">
                                            <field name="label">code</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">576</field>
                                        </record>
                                        <record id="iue_be_partial_retention_cash_payment_tax_balance" model="account.report.expression">
                                            <field name="label">balance</field>
                                            <field name="auditable" eval="False"/>
                                            <field name="engine">aggregation</field>
                                            <field name="formula">iueBep996.balance - iueBep677.balance</field>
                                            <field name="subformula">if_above(CUR(0))</field>
                                        </record>
                                    </field>
                                </record>
                            </field>
                        </record>
                    </field>
                </record>
            </field>
        </record>

    </data>
</odoo>