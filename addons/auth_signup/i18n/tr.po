"    ${user.signature | safe}\n"
"    <p style=\"font-size: 11px; margin-top: 10px;\">\n"
"        <strong>Sent by ${user.company_id.name} using <a href=\"www.odoo.com"
"\" style=\"text-decoration:none; color: #a24689;\">Odoo</a></strong>\n"
"    </p>\n"
"</div>"
msgstr ""

