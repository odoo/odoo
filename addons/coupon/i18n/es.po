"                % else\n"
"                <strong style=\"font-size: 24px;\">\n"
"                    ${'on %s' % object.program_id.discount_specific_product_ids.name}\n"
"                </strong>\n"
"                % endif\n"
"            % elif object.program_id.discount_apply_on == 'cheapest_product':\n"
