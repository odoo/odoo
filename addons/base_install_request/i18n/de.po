#. module: base_install_request
#: model:mail.template,body_html:base_install_request.mail_template_base_install_request
msgid ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"        Hello,\n"
"        <br/><br/>\n"
"        <span style=\"font-weight: bold;\" t-out=\"object.user_id.name\"/> "
"has requested to activate the <span style=\"font-weight: bold;\" t-"
"out=\"object.module_id.shortdesc\"/> module.\n"
"        <br/><br/>\n"
"        </p><blockquote data-o-mail-quote-node=\"1\" data-o-mail-"
"quote=\"1\">\n"
"            <t t-out=\"object.body_html\"/>\n"
"        </blockquote>\n"
"        <br data-o-mail-quote=\"1\"/><br data-o-mail-quote=\"1\"/>\n"
"        <a t-attf-style=\"background-color: "
"{{object.user_id.company_id.email_secondary_color or '#875A7B'}}; "
"padding:8px 16px 8px 16px; text-decoration:none; color: "
"{{object.user_id.company_id.email_primary_color or '#FFFFFF'}}; border-"
"radius:5px\" t-attf-href=\"/odoo/{{ object.module_id.id }}/action-"
"base_install_request.action_base_module_install_review?"
"menu_id={{ ctx['menu_id'] }}\">Review Request</a>\n"
"        <br/><br/>\n"
"        Thanks,\n"
"        <t t-if=\"not is_html_empty(object.user_id.signature)\" data-o-mail-"
"quote-container=\"1\">\n"
"            <br/><br/>\n"
"            <div data-o-mail-quote=\"1\">--<br data-o-mail-quote=\"1\"/><t t-"
"out=\"object.user_id.signature or ''\" data-o-mail-quote=\"1\">Mitchell "
"Admin</t></div>\n"
"        </t>\n"
"        <br/><br/>\n"
"    \n"
"</div>\n"
"            "
msgstr ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"        Hallo,\n"
"        <br/><br/>\n"
"        <span style=\"font-weight: bold;\" t-out=\"object.user_id.name\"/> "
"hat die Aktivierung des Moduls <span style=\"font-weight: bold;\" t-out=\""
"object.module_id.shortdesc\"/> beantragt.\n"
"        <br/><br/>\n"
"        </p><blockquote data-o-mail-quote-node=\"1\" data-o-mail-quote=\"1\">"
"\n"
"            <t t-out=\"object.body_html\"/>\n"
"        </blockquote>\n"
"        <br data-o-mail-quote=\"1\"/><br data-o-mail-quote=\"1\"/>\n"
"        <a t-attf-style=\"background-color: {{"
"object.user_id.company_id.email_secondary_color or '#875A7B'}}; padding:8px "
"16px 8px 16px; text-decoration:none; color: {{"
"object.user_id.company_id.email_primary_color or '#FFFFFF'}}; border-radius:"
"5px\" t-attf-href=\"/odoo/{{ object.module_id.id }}/action-"
"base_install_request.action_base_module_install_review?menu_id={{ ctx"
"['menu_id'] }}\">Anfrage prüfen</a>\n"
"        <br/><br/>\n"
"        Thanks,\n"
"        <t t-if=\"not is_html_empty(object.user_id.signature)\" data-o-mail-"
"quote-container=\"1\">\n"
"            <br/><br/>\n"
"            <div data-o-mail-quote=\"1\">--<br data-o-mail-quote=\"1\"/><t "
"t-out=\"object.user_id.signature or ''\" data-o-mail-quote=\"1\">Mitchell "
"Admin</t></div>\n"
"        </t>\n"
"        <br/><br/>\n"
"    \n"
"</div>\n"
"            "

