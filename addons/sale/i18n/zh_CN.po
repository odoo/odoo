"                    </tr>\n"
"                    <tr>\n"
"                        <td>\n"
"                            <strong>付款方式：</strong>\n"
"                            % if transaction.payment_token_id:\n"
"                                ${transaction.payment_token_id.name}\n"
"                            % else:\n"
"                                ${transaction.acquirer_id.name}\n"
"                            % endif\n"
"                             (${format_amount(transaction.amount, object.currency_id)})\n"
"                        </td>\n"
"                    </tr>\n"
