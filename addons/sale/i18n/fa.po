msgstr ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"        <t t-set=\"doc_name\" t-value=\"object.type_name\"/>\n"
"        اقا - خانم <t t-out=\"object.partner_id.name or ''\">کاربر</t>,\n"
"        <br/><br/>\n"
