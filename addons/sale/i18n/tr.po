"                % if object.carrier_id:\n"
"                    <tr>\n"
"                        <td style=\"border-top: 1px solid #dee2e6;\" align=\"right\"><strong>Teslim:</strong></td>\n"
"                        <td style=\"border-top: 1px solid #dee2e6;\" align=\"right\">${format_amount(object.amount_delivery, object.currency_id)}</td>\n"
"                    </tr>\n"
"                    <tr>\n"
"                        <td width=\"30%\" align=\"right\"><strong>Ara Toplam:</strong></td>\n"
