"                <strong>\n"
"                    ${line['product']}\n"
"                    (x${line['quantity']})\n"
"                 </strong>\n"
"                % if line['note']:\n"
"                    <em>(${line['note']})</em>\n"
"                % endif\n"
"                <div style=\"display: inline-block; float:right\">\n"
"                    ${format_amount(line['price'], currency)}\n"
"                </div>\n"
"            </li>\n"
"        % endfor\n"
"        </ul>\n"
"\n"
