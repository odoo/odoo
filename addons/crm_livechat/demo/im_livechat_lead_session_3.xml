<?xml version="1.0"?>
<odoo>
    <data>
        <record id="im_livechat_lead_session_3_channel_demo" model="discuss.channel">
            <field name="channel_type">livechat</field>
            <field name="livechat_channel_id" ref="im_livechat.im_livechat_channel_data"/>
            <field name="livechat_operator_id" ref="base.partner_admin"/>
            <field name="name">Visitor #789, Mitchell Admin</field>
            <field name="anonymous_name">Visitor #789</field>
            <field name="create_date" eval="DateTime.today()"/>
        </record>
        <record id="im_livechat_lead_session_3_member_admin_demo" model="discuss.channel.member">
            <field name="partner_id" ref="base.partner_admin"/>
            <field name="channel_id" ref="im_livechat_lead_session_3_channel_demo"/>
            <field name="unpin_dt" eval="DateTime.today()"/>
            <field name="last_interest_dt" eval="DateTime.today()"/>
            <field name="livechat_member_type">agent</field>
        </record>
        <record id="im_livechat_lead_session_3_guest_demo" model="mail.guest">
            <field name="name">Visitor #789</field>
        </record>
        <record id="im_livechat_lead_session_3_member_guest_demo" model="discuss.channel.member">
            <field name="guest_id" ref="im_livechat_lead_session_3_guest_demo"/>
            <field name="channel_id" ref="im_livechat_lead_session_3_channel_demo"/>
            <field name="livechat_member_type">visitor</field>
        </record>

        <record id="im_livechat_lead_session_3_message_1_demo" model="mail.message">
            <field name="model">discuss.channel</field>
            <field name="res_id" ref="im_livechat_lead_session_3_channel_demo"/>
            <field name="message_type">email</field>
            <field name="author_id" ref="base.partner_admin"/>
            <field name="body">Hi there! How may I assist you today?</field>
            <field name="message_type">comment</field>
            <field name="subtype_id" ref="mail.mt_comment"/>
            <field eval="DateTime.today() + relativedelta(minutes=1)" name="date"/>
        </record>
        <record id="im_livechat_lead_session_3_message_2_demo" model="mail.message">
            <field name="model">discuss.channel</field>
            <field name="res_id" ref="im_livechat_lead_session_3_channel_demo"/>
            <field name="author_guest_id" ref="im_livechat_lead_session_3_guest_demo"/>
            <field name="author_id"/>
            <field name="email_from">Visitor</field>
            <field name="body">I'm interested in your 'Marketing Campaign Package'.</field>
            <field name="message_type">comment</field>
            <field name="subtype_id" ref="mail.mt_comment"/>
            <field eval="DateTime.today() + relativedelta(minutes=2)" name="date"/>
        </record>
        <record id="im_livechat_lead_session_3_message_3_demo" model="mail.message">
            <field name="model">discuss.channel</field>
            <field name="res_id" ref="im_livechat_lead_session_3_channel_demo"/>
            <field name="author_id" ref="base.partner_admin"/>
            <field name="body">Excellent! That package is designed to boost your online presence. What kind of campaign are you looking for?</field>
            <field name="message_type">comment</field>
            <field name="subtype_id" ref="mail.mt_comment"/>
            <field eval="DateTime.today() + relativedelta(minutes=3)" name="date"/>
        </record>
        <record id="im_livechat_lead_session_3_message_4_demo" model="mail.message">
            <field name="model">discuss.channel</field>
            <field name="res_id" ref="im_livechat_lead_session_3_channel_demo"/>
            <field name="author_guest_id" ref="im_livechat_lead_session_3_guest_demo"/>
            <field name="author_id"/>
            <field name="email_from">Visitor</field>
            <field name="body">I need something to increase website traffic and generate more leads.</field>
            <field name="message_type">comment</field>
            <field name="subtype_id" ref="mail.mt_comment"/>
            <field eval="DateTime.today() + relativedelta(minutes=4)" name="date"/>
        </record>
        <record id="im_livechat_lead_session_3_message_5_demo" model="mail.message">
            <field name="model">discuss.channel</field>
            <field name="res_id" ref="im_livechat_lead_session_3_channel_demo"/>
            <field name="author_id" ref="base.partner_admin"/>
            <field name="body">Perfect! Our marketing specialist will reach out to discuss a tailored strategy.</field>
            <field name="message_type">comment</field>
            <field name="subtype_id" ref="mail.mt_comment"/>
            <field eval="DateTime.today() + relativedelta(minutes=5)" name="date"/>
        </record>

        <record id="im_livechat_lead_session_3_lead_demo" model="crm.lead">
            <field name="name">Marketing Campaign Package Inquiry</field>
            <field name="partner_name">Visitor #789</field>
            <field name="email_from">marketing.inquiry@example.com</field>
            <field name="origin_channel_id" ref="im_livechat_lead_session_3_channel_demo"/>
        </record>
    </data>
</odoo>
