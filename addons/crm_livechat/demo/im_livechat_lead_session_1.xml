<?xml version="1.0"?>
<odoo>
    <data>
        <record id="im_livechat_lead_session_1_channel_demo" model="discuss.channel">
            <field name="channel_type">livechat</field>
            <field name="livechat_channel_id" ref="im_livechat.im_livechat_channel_data"/>
            <field name="livechat_operator_id" ref="base.partner_admin"/>
            <field name="name">Visitor #278, Mitchell Admin</field>
            <field name="anonymous_name">Visitor #234</field>
            <field name="create_date" eval="DateTime.today() + relativedelta(months=-1)"/>
        </record>
        <record id="livechat_channel_session_2_member_demo" model="discuss.channel.member">
            <field name="partner_id" ref="base.partner_admin"/>
            <field name="channel_id" ref="im_livechat_lead_session_1_channel_demo"/>
            <field name="unpin_dt" eval="DateTime.today()"/>
            <field name="last_interest_dt" eval="DateTime.today() + relativedelta(months=-1)"/>
            <field name="livechat_member_type">agent</field>
        </record>
        <record id="im_livechat_lead_session_1_guest_demo" model="mail.guest">
            <field name="name">Visitor #278</field>
        </record>
        <record id="livechat_channel_lead_session_1_member_guest_demo" model="discuss.channel.member">
            <field name="guest_id" ref="im_livechat_lead_session_1_guest_demo"/>
            <field name="channel_id" ref="im_livechat_lead_session_1_channel_demo"/>
            <field name="livechat_member_type">visitor</field>
        </record>

        <record id="im_livechat_lead_session_1_message_1_demo" model="mail.message">
            <field name="model">discuss.channel</field>
            <field name="res_id" ref="im_livechat_lead_session_1_channel_demo"/>
            <field name="message_type">email</field>
            <field name="author_id" ref="base.partner_admin"/>
            <field name="body">Hello, how may I help you?</field>
            <field name="message_type">comment</field>
            <field name="subtype_id" ref="mail.mt_comment"/>
            <field eval="DateTime.today() + relativedelta(months=-1, days=-0, minutes=1)" name="date"/>
        </record>
        <record id="im_livechat_lead_session_1_message_2_demo" model="mail.message">
            <field name="model">discuss.channel</field>
            <field name="res_id" ref="im_livechat_lead_session_1_channel_demo"/>
            <field name="author_guest_id" ref="im_livechat_lead_session_1_guest_demo"/>
            <field name="author_id"/>
            <field name="email_from">Visitor</field>
            <field name="body">Hi, I'm interested in purchasing your 'Standing Desk'.</field>
            <field name="message_type">comment</field>
            <field name="subtype_id" ref="mail.mt_comment"/>
            <field eval="DateTime.today() + relativedelta(months=-1, days=-0, minutes=2)" name="date"/>
        </record>
        <record id="im_livechat_lead_session_1_message_3_demo" model="mail.message">
            <field name="model">discuss.channel</field>
            <field name="res_id" ref="im_livechat_lead_session_1_channel_demo"/>
            <field name="author_id" ref="base.partner_admin"/>
            <field name="body">Excellent choice! That's a very popular product. Would you like more information or a demo?</field>
            <field name="message_type">comment</field>
            <field name="subtype_id" ref="mail.mt_comment"/>
            <field eval="DateTime.today() + relativedelta(months=-1, days=-0, minutes=3)" name="date"/>
        </record>
        <record id="im_livechat_lead_session_1_message_4_demo" model="mail.message">
            <field name="model">discuss.channel</field>
            <field name="res_id" ref="im_livechat_lead_session_1_channel_demo"/>
            <field name="author_guest_id" ref="im_livechat_lead_session_1_guest_demo"/>
            <field name="author_id"/>
            <field name="email_from">Visitor</field>
            <field name="body">A demo would be great. How can I arrange that?</field>
            <field name="message_type">comment</field>
            <field name="subtype_id" ref="mail.mt_comment"/>
            <field eval="DateTime.today() + relativedelta(months=-1, days=-0, minutes=4)" name="date"/>
        </record>
        <record id="im_livechat_lead_session_1_message_5_demo" model="mail.message">
            <field name="model">discuss.channel</field>
            <field name="res_id" ref="im_livechat_lead_session_1_channel_demo"/>
            <field name="author_id" ref="base.partner_admin"/>
            <field name="body">One of our sales representatives will contact you shortly to arrange a demo!</field>
            <field name="message_type">comment</field>
            <field name="subtype_id" ref="mail.mt_comment"/>
            <field eval="DateTime.today() + relativedelta(months=-1, days=-0, minutes=5)" name="date"/>
        </record>
        <record id="im_livechat_lead_session_1_message_7_demo" model="mail.message">
            <field name="model">discuss.channel</field>
            <field name="res_id" ref="im_livechat_lead_session_1_channel_demo"/>
            <field name="message_type">email</field>
            <field name="author_guest_id" ref="im_livechat_lead_session_1_guest_demo"/>
            <field name="author_id"/>
            <field name="body">Thank you!</field>
            <field name="message_type">comment</field>
            <field eval="DateTime.today() + relativedelta(months=-1, days=-0, minutes=7)" name="date"/>
        </record>

        <record id="im_livechat_lead_session_1_lead_demo" model="crm.lead">
            <field name="name">Standing Desk Demo</field>
            <field name="partner_name">Visitor #234</field>
            <field name="email_from">visitor@example.com</field>
            <field name="origin_channel_id" ref="im_livechat_lead_session_1_channel_demo"/>
        </record>
    </data>
</odoo>
