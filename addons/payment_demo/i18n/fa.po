#. module: payment_demo
#: model_terms:ir.ui.view,arch_db:payment_demo.payment_details
msgid ""
"<select id=\"simulated_payment_state\" class=\"form-select\">\n"
"                    <option value=\"done\" title=\"Successful payment\">\n"
"                        Successful\n"
"                    </option>\n"
"                    <option value=\"pending\" title=\"Payment processing\">\n"
"                        Pending\n"
"                    </option>\n"
"                    <option value=\"cancel\" title=\"Payment canceled by customer\">\n"
"                        Canceled\n"
"                    </option>\n"
"                    <option value=\"error\" title=\"Processing error\">\n"
"                        Error\n"
"                    </option>\n"
"                </select>"
msgstr ""
"```html\n"
"<select id=\"simulated_payment_state\" class=\"form-select\">\n"
"            <option value=\"done\" title=\"پرداخت موفق\">\n"
"                موفق\n"
"            </option>\n"
"            <option value=\"pending\" title=\"پردازش پرداخت\">\n"
"                در حال پردازش\n"
"            </option>\n"
"            <option value=\"cancel\" title=\"پرداخت توسط مشتری لغو شد\">\n"
"                لغو شده\n"
"            </option>\n"
"            <option value=\"error\" title=\"خطای پردازش\">\n"
"                خطا\n"
"            </option>\n"
"        </select>\n"
"```"

