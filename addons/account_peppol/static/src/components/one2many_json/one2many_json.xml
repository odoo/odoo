<?xml version="1.0" encoding="UTF-8" ?>
<template>
    <t t-name="account.One2ManyJson">
        <t t-set="items" t-value="state.items"/>
        <div class="o_inner_group grid col-lg-6 w-100">
            <t t-foreach="items" t-as="item" t-key="item">
                <t t-if="item_value['sequence'] &lt; this.sequence_threshold">
                    <div class="o_cell o_wrap_label text-break text-900">
                        <label t-att-for="item" t-out="item_value['label']" class="o_form_label"/>
                    </div>
                    <div class="o_cell o_wrap_input text-break mb-3 mb-sm-0">
                        <input t-att-id="item" type="text" class="o_input o_field_widget o_field_char" t-att-value="item_value['value']" t-on-change="(ev) => this.onChange(item, ev.target.value)"/>
                    </div>
                </t>
            </t>
        </div>
    </t>
</template>
