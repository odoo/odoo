"                <div data-o-mail-quote=\"1\">--<br data-o-mail-quote=\"1\"/"
"><t t-out=\"user.signature or ''\">Mitchell Admin</t></div>\n"
"            </td>\n"
"            <td t-else=\"\" style=\"padding:15px 20px 10px 20px;\">\n"
"                <t t-out=\"user.company_id.name or ''\"/>\n"
"            </td>\n"
"        </tr>\n"
"    </tbody>\n"
" </table>\n"
"</div>"
msgstr ""

