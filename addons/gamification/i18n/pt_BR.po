"                    <tr>\n"
"                        <div>\n"
"                            % if object.visibility_mode == 'ranking':\n"
"                                <td style=\"padding:15px;\">\n"
"                                    <p style=\"font-size:20px;color:#666666;\" align=\"center\">Leaderboard</p>\n"
"                                </td>\n"
"                            % endif\n"
"                        </div>\n"
