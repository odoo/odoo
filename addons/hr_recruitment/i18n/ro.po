msgid ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"    <div style=\"text-align: center\">\n"
"        <h2>Congratulations!</h2>\n"
"        <div style=\"color:grey;\">Your resume has been positively reviewed."
"</div>\n"
