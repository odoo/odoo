<?xml version="1.0" encoding="utf-8"?>
<odoo>

<record id="fiscal_position_eu" model="account.fiscal.position.template">
    <field name="name">Europos Sąjungos vidus</field>
    <field name="chart_template_id" ref="l10n_lt_chart_template"/>
    <field name="note">PVM įstatymo 49 straipsnio 1 dalis (prekės tiekiamos į kitą valstybę narę) − Direktyvos 2006/112/EB 138 (1) straipsnis / 2006/112/EB Directive, 138 (1) (a) art. Goods are supplied to other European Union country</field>
</record>

<record id="fiscal_position_tax_eu_sale" model="account.fiscal.position.tax.template">
    <field name="position_id" ref="fiscal_position_eu"/>
    <field name="tax_src_id" ref="LT_SALE_21"/>
    <field name="tax_dest_id" ref="EU_SALE_0"/>
</record>
<record id="fiscal_position_tax_eu_purchase" model="account.fiscal.position.tax.template">
    <field name="position_id" ref="fiscal_position_eu"/>
    <field name="tax_src_id" ref="LT_PURCHASE_21"/>
    <field name="tax_dest_id" ref="EU_PURCHASE_0"/>
</record>


<record id="fiscal_position_non_eu" model="account.fiscal.position.template">
    <field name="name">Už Europos Sąjungos ribų</field>
    <field name="chart_template_id" ref="l10n_lt_chart_template"/>
    <field name="note">PVM istatymo 41 straipsnio 1 dalis ( prekes eksportuojamos į trečiąją valstybe tiekėjo ar jo užsakymu kito asmens) Direktyvos
2006/112/EB 146 (1) (a) straipsnis.
VAT law 41 atr. part 1. Services are exported to other country 2006/112/EB Directive, 146 (1) (a) art.</field>
</record>

<record id="fiscal_position_tax_non_eu_sale" model="account.fiscal.position.tax.template">
    <field name="position_id" ref="fiscal_position_non_eu"/>
    <field name="tax_src_id" ref="LT_SALE_21"/>
    <field name="tax_dest_id" ref="LT_SALE_0"/>
</record>
<record id="fiscal_position_tax_non_eu_purchase" model="account.fiscal.position.tax.template">
    <field name="position_id" ref="fiscal_position_non_eu"/>
    <field name="tax_src_id" ref="LT_PURCHASE_21"/>
    <field name="tax_dest_id" ref="LT_PURCHASE_0"/>
</record>

</odoo>
