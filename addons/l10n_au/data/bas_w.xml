                                                        <field name="engine">tax_tags</field>
                                                        <field name="formula">-G7</field>
                                                    </record>
                                                </field>
                                            </record>
                                        </field>
                                    </record>
                                    <record id="l10n_au_bas_w_g8" model="account.report.line">
