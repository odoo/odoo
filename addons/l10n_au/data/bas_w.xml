                                                <field name="label">balance</field>
                                                <field name="engine">aggregation</field>
                                                <field name="formula">G8.balance/11</field>
                                            </record>
                                        </field>
                                    </record>
                                </field>
                            </record>
                            <record id="l10n_au_bas_w_gst_paid_calc" model="account.report.line">
