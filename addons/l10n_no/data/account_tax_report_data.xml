        <field name="code">NOTAX_0100</field>
        <field name="sequence" eval="0"/>
        <field name="parent_id" ref="account_tax_report_line_sum_avgiftsgrunnlag"/>
        <field name="country_id" ref="base.no"/>
    </record>

