"                        }\n"
"\n"
"                        70% {\n"
"                            transform: scale(1);\n"
"                        }\n"
"                    }\n"
"                .swing {\n"
"                    animation: swing 1.3s ease forwards;\n"
"                    animation-delay: 0.75s;\n"
"                    transform-origin: top center\n"
"                }\n"
"                @keyframes swing {\n"
"                    20% {\n"
"                    transform: rotate(15deg)\n"
"                }\n"
"\n"
"                40% {\n"
"                    transform: rotate(-10deg)\n"
"                }\n"
"\n"
"                60% {\n"
"                    transform: rotate(5deg)\n"
"                }\n"
"\n"
"                80% {\n"
"                    transform: rotate(-5deg)\n"
"                }\n"
