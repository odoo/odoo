<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

    <t t-name="point_of_sale.OrderlineNoteButton">
        <button class="btn btn-light rounded-0 fw-bolder" t-att-disabled="!pos.get_order()?.get_selected_orderline()" t-on-click="onClick">
