            <field name="date_planned" eval="time.strftime('%Y-%m-%d %H:%M:%S')"/>
            <field name="location_dest_id" ref="stock.stock_location_customers"/>
            <field model="stock.picking" name="picking_id" search="[('name', '=', u'OUT/00019')]"/>
        </record>
