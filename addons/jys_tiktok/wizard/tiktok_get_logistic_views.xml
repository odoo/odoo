<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_tiktok_get_logistic_form" model="ir.ui.view">
            <field name="name">Tiktok Get Logistic</field>
            <field name="model">tiktok.get.logistic</field>
            <field name="arch" type="xml">
                <form string="Tiktok Get logistic">
                    <p>
                        Integration Tiktok to System.
                    </p>
                    <separator string="Shops"/>
                    <field name="shop_id" nolabel="1" required="1"/>
                    <footer>
                        <button name="action_confirm" string="Confirm" type="object" class="oe_highlight"/>
                        <button string="Cancel" class="oe_link" special="cancel" />
                    </footer>
                </form>
            </field>
        </record>

        <record id="action_jys_tiktok_get_logistic" model="ir.actions.act_window">
            <field name="name">Get Logistic</field>
            <field name="res_model">tiktok.get.logistic</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="view_tiktok_get_logistic_form"/>
            <field name="target">new</field>
        </record>

    </data>
</odoo>
