<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="ubl_invoice_e_fff" name="UBL invoice protocol e-fff" inherit_id="account_ubl.ubl_invoice" primary="True">
            <xpath expr=".//cbc__DocumentCurrencyCode[1]" position="after">
                <cbc__LineCountNumeric t-esc="line_count"/>
                <cac__AdditionalDocumentReference> 
                    <cbc__ID t-esc="doc_ref_id"/>
                    <cac__Attachment>
                        <cbc__EmbeddedDocumentBinaryObject 
                            mimeCode="application/pdf"
                            t-esc="attach_binary"/>
                    </cac__Attachment>
                </cac__AdditionalDocumentReference>
            </xpath>
        </template>
    </data>
</odoo>