<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

        <!-- Could maybe removed with api change -->
        <record id="credit_account" model="account.account">
            <field name="name">Bank Account</field>
            <field name="user_type" ref="account.data_account_type_bank"/>
            <field name="code">X500</field>
        </record>


        <record id="bank_journal" model="account.journal">
            <field name="name">Bank Journal</field>
            <field name="code">TBNK</field>
            <field name="type">bank</field>
            <field name="default_credit_account_id" ref="credit_account"/>
            <field name="default_debit_account_id" ref="credit_account"/>
        </record>

        <record id="demo_bank_account" model="res.partner.bank">
            <field name="state">bank</field>
            <field name="acc_number">BE33737018595246</field>
            <field name="bank_bic">KREDBEBB</field>
            <field name="partner_id" ref="base.main_partner"/>
            <field name="journal_id" ref="bank_journal"/>
        </record>

        <!--
        Fiscal year
        -->

        <record id="data_fiscalyear_2011" model="account.fiscalyear">
            <field eval="'Fiscal Year 2011'" name="name"/>
            <field eval="'2011-01-01'" name="date_start"/>
            <field eval="'2011-12-31'" name="date_stop"/>
            <field name="company_id" ref="base.main_company"/>
        </record>

        
    </data>
</openerp>
