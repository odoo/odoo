#. module: mail_group
#: model:mail.template,body_html:mail_group.mail_template_list_subscribe
msgid ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"                Hello,<br/><br/>\n"
"                You have requested to be subscribed to the mailing list "
"<strong t-out=\"object.name or ''\"/>.\n"
"                <br/><br/>\n"
"                To confirm, please visit the following link: <strong t-"
"if=\"ctx.get('token_url')\"><a t-att-href=\"ctx['token_url']\"><t t-"
"out=\"ctx['token_url'] or ''\"/></a></strong>\n"
"                <br/><br/>\n"
"                If this was a mistake or you did not requested this action, "
"please ignore this message.\n"
"            </div>\n"
"        "
msgstr ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"                नमस्ते,<br/><br/>\n"
"                आपने मेलिंग लिस्ट <strong t-out=\"object.name or ''\"/> को सब्सक्राइब "
"करने का अनुरोध किया है.\n"
"                <br/><br/>\n"
"                पुष्टि करने के लिए, कृपया इस लिंक पर जाएं: <strong t-if="
"\"ctx.get('token_url')\"><a t-att-href=\"ctx['token_url']\"><t t-out="
"\"ctx['token_url'] or ''\"/></a></strong>\n"
"                <br/><br/>\n"
"                अगर यह हमारी गलती है या आपने इसके लिए अनुरोध नहीं किया है, तो कृपया इस मैसेज पर "
"ध्यान ना दें.\n"
"            </div>\n"
"        "

#. module: mail_group
#: model:mail.template,body_html:mail_group.mail_template_list_unsubscribe
msgid ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"                Hello,<br/><br/>\n"
"                You have requested to be unsubscribed to the mailing list "
"<strong t-out=\"object.name or ''\"/>.\n"
"                <br/><br/>\n"
"                To confirm, please visit the following link: <strong t-"
"if=\"ctx.get('token_url')\"><a t-att-href=\"ctx['token_url']\"><t t-"
"out=\"ctx['token_url'] or ''\"/></a></strong>.\n"
"                <br/><br/>\n"
"                If this was a mistake or you did not requested this action, "
"please ignore this message.\n"
"            </div>\n"
"        "
msgstr ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"                नमस्ते,<br/><br/>\n"
"                आपने मेलिंग लिस्ट <strong t-out=\"object.name or ''\"/> को अनसब्सक्रा"
"इब करने का अनुरोध किया है.\n"
"                <br/><br/>\n"
"                पुष्टि करने के लिए, कृपया इस लिंक पर जाएं: <strong t-if="
"\"ctx.get('token_url')\"><a t-att-href=\"ctx['token_url']\"><t t-out="
"\"ctx['token_url'] or ''\"/></a></strong>.\n"
"                <br/><br/>\n"
"                अगर यह हमारी कोई गलती है या फिर आपने अनसब्सक्राइब करने का अनुरोध नहीं किया है, तो "
"कृपया इस मैसेज पर ध्यान ना दें.\n"
"            </div>\n"
"        "

#. module: mail_group
#: model:mail.template,body_html:mail_group.mail_template_guidelines
msgid ""
"<div>\n"
"                <p>Hello <t t-out=\"object.partner_id.name or ''\"/>,</p>\n"
"                <p>Please find below the guidelines of the <t t-"
"out=\"object.mail_group_id.name\"/> mailing list.</p>\n"
"                <p><t t-out=\"object.mail_group_id.moderation_guidelines_msg "
"or ''\"/></p>\n"
"            </div>\n"
"        "
msgstr ""
"<div>\n"
"                <p>नमस्ते <t t-out=\"object.partner_id.name or ''\"/>,</p>\n"
"                <p>कृपया नीचे <t t-out=\"object.mail_group_id.name\"/> मेलिंग लिस्ट के "
"दिशानिर्देश देखें.</p>\n"
"                <p><t t-out="
"\"object.mail_group_id.moderation_guidelines_msg or ''\"/></p>\n"
"            </div>\n"
"        "

