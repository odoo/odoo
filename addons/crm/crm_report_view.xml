                    <field name="nbr"/>
                    <field name="state"/>
                    <field name="section_id" groups="base.group_multi_salesteams"/>
                    <field name="category2_id"/>
               </search>
            </field>
        </record>

<!--  Cases by section and category2 of case Action -->

