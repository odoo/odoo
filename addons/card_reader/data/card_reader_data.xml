<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="barcode_rule_credit" model="barcode.rule">
            <field name="name">Magnetic Credit Card</field>
            <field name="barcode_nomenclature_id" ref="barcodes.default_barcode_nomenclature"/>
            <field name="sequence">85</field>
            <field name="type">Credit</field>
            <field name="encoding">magnetic_credit</field>
            <field name="pattern">%.*</field>
        </record>

        <record id="card_reader_configuration" model="card_reader.configuration">
            <field name="merchant_id">334160</field>
            <field name="merchant_pwd">81303DUR</field>
            <field name="memo">Odoo Test</field>
            <field name="payment_server">https://w1.mercurypay.com/ws/ws.asmx</field>
            <field name="url_base_action">http://www.mercurypay.com</field>
        </record>

        <record model="pos.config" id="point_of_sale.pos_config_main">
            <field name="card_reader" eval="True" />
            <field name="operator_id">Operator 1</field>
            <field name="card_reader_config_id" ref="card_reader.card_reader_configuration" />
        </record>

    </data>
</openerp>
