        t-name="sale_pdf_quote_builder.QuotationDocumentX2ManyField"
        t-inherit-mode="primary"
        t-inherit="web.X2ManyField"
    >
        <xpath expr="//div[hasclass('o_cp_buttons')]" position="inside">
            <UploadButton
                formData="formData"
                allowedMIMETypes="allowedMIMETypes"
