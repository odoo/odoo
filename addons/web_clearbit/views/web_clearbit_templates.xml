<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="assets_backend" name="Web Clearbit Assets" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <link rel="stylesheet" type="text/scss" href="/web_clearbit/static/src/scss/web_clearbit.scss"/>
            <script type="text/javascript" src="/web_clearbit/static/src/js/clearbit_core.js" />
            <script type="text/javascript" src="/web_clearbit/static/src/js/res_partner_autocomplete.js" />
            <script type="text/javascript" src="/web_clearbit/static/src/js/res_partner_many2one_clearbit.js" />
        </xpath>
    </template>

    <template id="qunit_suite" inherit_id="web.qunit_suite">
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/web_clearbit/static/tests/web_clearbit_tests.js"/>
        </xpath>
    </template>
</odoo>
