<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <t t-name="website_partner.PartnersPageOption">
        <BuilderRow t-if="state.has_google_maps_api_key" label.translate="World Map">
            <BuilderCheckbox action="'websiteConfig'" actionParam="{views: ['website_partner.ref_country']}" />
        </BuilderRow>
        <BuilderRow label.translate="Address">
            <BuilderCheckbox action="'websiteConfig'" actionParam="{views: ['website_partner.o_wcrm_partner_address']}" />
        </BuilderRow>
        <BuilderRow label.translate="Companies only">
            <BuilderCheckbox action="'websiteConfig'" actionParam="{views: ['website_partner.companies_only']}" />
        </BuilderRow>
        <BuilderRow label.translate="Search">
            <BuilderCheckbox action="'websiteConfig'" actionParam="{views: ['website_partner.search']}" />
        </BuilderRow>
        <BuilderRow label.translate="Countries">
            <BuilderCheckbox action="'websiteConfig'" actionParam="{views: ['website_partner.countries']}" />
        </BuilderRow>
        <BuilderRow label.translate="Partner page">
            <BuilderCheckbox action="'websiteConfig'" actionParam="{views: ['website_partner.partner_page']}" />
        </BuilderRow>
    </t>

</templates>
