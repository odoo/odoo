<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="view_partner_form_l10n_il" model="ir.ui.view">
            <field name="name">l10n_il.partner.form</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='vat']" position="before">
                  <field name="l10n_il_income_tax_id_number"/>
                  <field name="l10n_il_registry_number"/>
                  <field name="l10n_il_withh_tax_reason"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
