<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="view_company_form_l10n_il" model="ir.ui.view">
            <field name="name">l10n_il.company.form</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="base.view_company_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='email']" position="after">
                  <field name="l10n_il_income_tax_id_number"/>
                  <field name="l10n_il_registry_number"/>
                  <field name="l10n_il_withh_tax_id_number"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
