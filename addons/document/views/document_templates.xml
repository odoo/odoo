<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <template id="assets_backend" name="document assets" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <script type="text/javascript" src="/document/static/src/js/document.js"></script>
            </xpath>
        </template>

        <template id="qunit_suite" name="document tests" inherit_id="web.qunit_suite">
            <xpath expr="//script[last()]" position="after">
                <script type="text/javascript" src="/document/static/tests/document_tests.js"/>
            </xpath>
        </template>
</odoo>
