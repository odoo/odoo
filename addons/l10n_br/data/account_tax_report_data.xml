                    <record id="tax_report_icmsst_2" model="account.tax.report.line">
                        <field name="name">ICMSST tax</field>
                        <field name="tag_name">ICMSST_2</field>
                        <field name="sequence" eval="2"/>
                    </record>
                </field>
            </record>
