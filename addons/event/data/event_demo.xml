        <field name="user_id" ref="base.user_admin"/>
    </record>
    <record id="event_2_mail_0" model="event.mail">
        <field name="event_id" ref="event.event_2"/>
        <field name="template_ref" eval="'mail.template,%i' % ref('event.event_subscription')"/>
    </record>

