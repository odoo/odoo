"                                    <p style=\"margin:0px 0px 0px 0px;\">${object.event_id.address_id.country_id.name}</p>\n"
"                                    % set location = '%s, %s' % (location, object.event_id.address_id.country_id.name)\n"
"                                % endif\n"
