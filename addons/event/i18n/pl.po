"out=\"object.event_id.address_id.state_id.name\">C1</t>,\n"
"                                        <t t-set=\"location\" t-"
"valuef=\"{{location}}, {{object.event_id.address_id.state_id.name}}\"/>\n"
"                                    </t>\n"
"                                    <t t-"
"if=\"object.event_id.address_id.zip\">\n"
