"                                        <div t-"
"out=\"object.event_id.address_id.name\">Teksa SpA</div>\n"
"                                    </t>\n"
"                                    <t t-"
"if=\"object.event_id.address_id.street\">\n"
"                                        <div t-"
"out=\"object.event_id.address_id.street\">Puerto Madero 9710</div>\n"
