"                            <t t-"
"out=\"object.sale_order_line_id.price_total\" t-options=\"{'widget': "
"'monetary', 'display_currency': object.sale_order_line_id.currency_id}\"/>\n"
"                        </t>.\n"
"                    </div>\n"
"                    <div>\n"
