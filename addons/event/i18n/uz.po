"options=\"{&quot;widget&quot;: &quot;datetime&quot;, &quot;time_only&quot;: "
"True, &quot;tz_name&quot;: object.event_id.date_tz, "
"&quot;hide_seconds&quot;: True, &quot;format&quot;: &quot;short&quot;}"
"\">5:00 PM</t>\n"
