    </record>

    <!-- ****** Price list ****** -->
    <record id="list_event_deals" model="product.pricelist">
        <field name="currency_id" ref="base.USD"/>
        <field name="discount_policy">without_discount</field>
        <field name="name">Event Deals</field>
        <field name="sequence">30</field>
    </record>

    <record id="item_event_vip_event" model="product.pricelist.item">
        <field name="applied_on">0_product_variant</field>
        <field name="base">list_price</field>
        <field name="compute_price">formula</field>
        <field name="min_quantity">2</field>
        <field name="price_discount">20</field>
        <field name="pricelist_id" ref="event_sale.list_event_deals"/>
        <field name="product_id" ref="event_sale.product_product_sport_event"/>
    </record>

