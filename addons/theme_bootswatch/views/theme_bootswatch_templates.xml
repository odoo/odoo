<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="theme_customize" inherit_id="website.theme_customize">
        <xpath expr="div" position="replace">
            <div role="dialog" id="theme_customize_modal" class="modal fade bs-example-modal-sm">
                <div class="modal-dialog">
                <div class="modal-content">
                    <div class="loading_backdrop"></div>
                        <header class="modal-header">
                            <button type="button" class="close">Ã—</button>
                            <h4 class="modal-title">Customize your theme</h4>
                        </header>
                        <main class="modal-body">
                            <div class="row">
                                <div class="col-xs-4 col-xs-offset-4">
                                    <label class="checked">
                                        <img style="width: 120px; height: 70px;" src="/theme_bootswatch/static/src/img/bootswatch_default_thumbnail.png" alt="Default Theme"/>
                                        <input name="theme" type="radio" data-xmlid="" checked="checked"/>
                                    </label>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-xs-4">
                                    <label>
                                        <img style="width: 120px; height: 70px;" src="/theme_bootswatch/static/src/img/cerulean_thumbnail.png" alt="Cerulean"/>
                                        <input name="theme" type="radio" data-xmlid="theme_bootswatch.theme_cerulean_bs_variables,theme_bootswatch.theme_cerulean"/>
                                    </label>
                                </div>
                                <div class="col-xs-4">
                                    <label>
                                        <img style="width: 120px; height: 70px;" src="/theme_bootswatch/static/src/img/cosmo_thumbnail.png" alt="Cosmo"/>
                                        <input name="theme" type="radio" data-xmlid="theme_bootswatch.theme_cosmo_bs_variables,theme_bootswatch.theme_cosmo" data-disable="ee"/>
                                    </label>
                                </div>
                                <div class="col-xs-4">
                                    <label>
                                        <img style="width: 120px; height: 70px;" src="/theme_bootswatch/static/src/img/cyborg_thumbnail.png" alt="Cyborg"/>
                                        <input name="theme" type="radio" data-xmlid="theme_bootswatch.theme_cyborg_bs_variables,theme_bootswatch.theme_cyborg"/>
                                    </label>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-xs-4">
                                    <label>
                                        <img style="width: 120px; height: 70px;" src="/theme_bootswatch/static/src/img/flatly_thumbnail.png" alt="Flatly"/>
                                        <input name="theme" type="radio" data-xmlid="theme_bootswatch.theme_flatly_bs_variables,theme_bootswatch.theme_flatly"/>
                                    </label>
                                </div>
                                <div class="col-xs-4">
                                    <label>
                                        <img style="width: 120px; height: 70px;" src="/theme_bootswatch/static/src/img/journal_thumbnail.png" alt="Journal"/>
                                        <input name="theme" type="radio" data-xmlid="theme_bootswatch.theme_journal_bs_variables,theme_bootswatch.theme_journal"/>
                                    </label>
                                </div>
                                <div class="col-xs-4">
                                    <label>
                                        <img style="width: 120px; height: 70px;" src="/theme_bootswatch/static/src/img/simplex_thumbnail.png" alt="Simplex"/>
                                        <input name="theme" type="radio" data-xmlid="theme_bootswatch.theme_simplex_bs_variables,theme_bootswatch.theme_simplex"/>
                                    </label>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-xs-4">
                                    <label>
                                        <img style="width: 120px; height: 70px;" src="/theme_bootswatch/static/src/img/slate_thumbnail.png" alt="Slate"/>
                                        <input name="theme" type="radio" data-xmlid="theme_bootswatch.theme_slate_bs_variables,theme_bootswatch.theme_slate"/>
                                    </label>
                                </div>
                                <div class="col-xs-4">
                                    <label>
                                        <img style="width: 120px; height: 70px;" src="/theme_bootswatch/static/src/img/spacelab_thumbnail.png" alt="Spacelab"/>
                                        <input name="theme" type="radio" data-xmlid="theme_bootswatch.theme_spacelab_bs_variables,theme_bootswatch.theme_spacelab"/>
                                    </label>
                                </div>
                                <div class="col-xs-4">
                                    <label>
                                        <img style="width: 120px; height: 70px;" src="/theme_bootswatch/static/src/img/united_thumbnail.png" alt="United"/>
                                        <input name="theme" type="radio" data-xmlid="theme_bootswatch.theme_united_bs_variables,theme_bootswatch.theme_united"/>
                                    </label>
                                </div>
                            </div>
                        </main>
                    </div>
                </div>
            </div>
        </xpath>
    </template>

    <template id="theme_cerulean_bs_variables" name="Cerulean" inherit_id="web._assets_frontend_helpers" active="False">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/theme_bootswatch/static/lib/bootswatch/cerulean/_variables.scss"/>
        </xpath>
    </template>
    <template id="theme_cerulean" name="Cerulean" inherit_id="website.assets_frontend" active="False">
