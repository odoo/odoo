<?xml version="1.0" encoding="UTF-8"?>

<templates>

    <t t-name="stockReports.buttons">
        <button type="button" class='btn btn-primary o_stock-widget-pdf'>PRINT</button>
    </t>

    <div role="dialog" t-name='stockReports.errorModal' class="modal" id="editable_error" tabindex="-1" data-bs-backdrop="static" style="z-index:9999;">
        <div class="modal-dialog modal-sm">
            <div class="modal-content">
                <header class="modal-header">
                    <h3 class="modal-title">Error</h3>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </header>
                <main class="modal-body">
                    <p id='insert_error' class='text-center'></p>
                </main>
            </div>
        </div>
    </div>

</templates>
