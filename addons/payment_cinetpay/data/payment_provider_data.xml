            <field name="cinetpay_site_id">105891419</field> <!-- ID du site CinetPay -->
            <field name="cinetpay_notify_url">https://votre-domaine.com/payment/cinetpay/notify</field> <!-- URL de notification -->
        </record>
    </data>
</odoo>
