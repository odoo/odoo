msgid ""
"\n"
"                {{ object.company_id.name }} : Your order with reference: "
"{{ object.pos_reference }} was processed succesfully with amount "
"{{  object.currency_id.format(object.amount_total) }}. Use "
"{{ object.pos_reference }}  for further reference\n"
"            "
