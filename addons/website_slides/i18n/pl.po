msgid ""
"<div style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"        Hello<br><br>\n"
"        You have been invited to join a new course: <t t-out=\"object.channel_id.name or ''\">Basics of Gardening</t>.\n"
"    </p>\n"
"</div>\n"
"            "
msgstr ""
"<div style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"<p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"Witaj<br><br>\n"
"Zostałeś zaproszony do udziału w nowym kursie: <t t-out=\"object.channel_id.name or ''\">Podstawy Ogrodnictwa</t>.\n"
