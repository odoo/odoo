#. module: website_slides
#: model:mail.template,body_html:website_slides.slide_template_shared
msgid ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"                    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\""
">\n"
"                        Hello<br/><br/>\n"
"                        ${user.name} shared the ${object.slide_type} <strong"
">${object.name}</strong> with you!\n"
"                        <div style=\"margin: 16px 8px 16px 8px; text-align:"
" center;\">\n"
"                            <a href=\"${(object.website_url +"
" '?fullscreen=1') if ctx['fullscreen'] else object.website_url | safe}\">\n"
"                                <img alt=\"${object.name}\""
" src=\"${ctx['base_url']}/web/image/slide.slide/${object.id}/image_1024\""
" style=\"height:auto; width:150px; margin: 16px;\"/>\n"
"                            </a>\n"
"                        </div>\n"
"                        <div style=\"margin: 16px 8px 16px 8px; text-align:"
" center;\">\n"
"                            <a href=\"${(object.website_url +"
