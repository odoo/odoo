<?xml version="1.0" encoding="UTF-8"?>

<templates xml:space="preserve">
    <t t-name="website_slides.search_items_slide_channels">
        <t t-foreach="bucket.data" t-as="result" t-key="result_index">
            <t t-call="website.search_result_item">
                <t t-set="search_item_metadata" t-value="result['total_time']"/>
            </t>
        </t>
    </t>

    <t t-name="website_slides.search_items_slide_slides">
        <t t-foreach="bucket.data" t-as="result" t-key="result_index">
            <t t-call="website.search_result_item"/>
        </t>
    </t>
</templates>
