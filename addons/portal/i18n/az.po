"                        <div style=\"margin: 16px 0px 16px 0px; text-align: "
"center;\">\n"
"                            <a t-att-href=\"signup_url\" style=\"display: "
"inline-block; padding: 10px; text-decoration: none; font-size: 12px; "
"background-color: #875A7B; color: #fff; border-radius: 5px;\">\n"
"                                <strong>Activate Account</strong>\n"
"                            </a>\n"
"                        </div>\n"
"                        <t t-out=\"object.wizard_id.welcome_message or "
"''\">Welcome to our company's portal.</t>\n"
