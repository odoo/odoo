"  <input type=\"hidden\" name=\"amount\" value=\"${amount}\"/>\n"
"  <input type=\"hidden\" name=\"currency_code\" "
"value=\"${currency.name}\"/>\n"
"  <input type=\"image\" name=\"submit\" "
"src=\"https://www.paypal.com/en_US/i/btn/btn_paynowCC_LG.gif\"/>\n"
"</form>\n"
"% endif\n"
"            "
