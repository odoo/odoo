    <record id="res_partner_location_3" model="res.partner">
        <field name="name">Brussels Expo</field>
        <field name="is_company">1</field>
        <field name="street">Place de Belgique 1</field>
        <field name="city">Brussels</field>
        <field name="zip">1020</field>
