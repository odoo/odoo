<?xml version="1.0" encoding="UTF-8"?>
<templates>
    <t t-name="hr_timesheet.TimesheetCalendarMyTimesheetsSidePanel" t-inherit="web.CalendarSidePanel" t-inherit-mode="primary">
        <xpath expr="//div[hasclass('btn-group')]/button[2]" position="attributes">
            <attribute name="data-tooltip">Click on a day or select a range of days to generate timesheets for the selected project on the chosen dates</attribute>
        </xpath>
        <xpath expr="//div[hasclass('btn-group')]/button[3]" position="attributes">
            <attribute name="data-tooltip">Click on a day or select a range of days to permanently delete its contents</attribute>
        </xpath>
    </t>
</templates>
