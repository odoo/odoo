<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <!-- vehicle brand -->
    <record id="brand_leyland" model="fleet.vehicle.model.brand">
        <field name="name">Ashok Leyland</field>
        <field name="image_128" type="base64" file="stock_transport/static/img/Trailer.png"/>
    </record>
    <record id="brand_tata" model="fleet.vehicle.model.brand">
        <field name="name">TATA</field>
      	<field name="image_128" type="base64" file="stock_transport/static/img/ACE.png"/>
    </record>

    <!-- vehicle category -->
    <record id="model_category_5" model="fleet.vehicle.model.category">
        <field name="name">Mini Truck</field>
        <field name="sequence">1</field>
        <field name="max_weight">750</field>
        <field name="max_volume">80</field>
    </record>
    <record id="model_category_7" model="fleet.vehicle.model.category">
        <field name="name">Multi Axel 32' Trailer</field>
        <field name="sequence">3</field>
        <field name="max_weight">1000</field>
        <field name="max_volume">80</field>
    </record>

    <!-- vehicle models -->
    <record id="model_trailer" model="fleet.vehicle.model">
          <field name="name">Trailer</field>
          <field name="brand_id" ref="brand_leyland"/>
          <field name="vehicle_type">car</field>
          <field name="category_id" ref="model_category_7"/>
          <field name="model_year">2018</field>
          <field name="default_fuel_type">diesel</field>
          <field name="transmission">manual</field>
          <field name="default_co2">195</field>
          <field name="power">600</field>
          <field name="horsepower">6000</field>
    </record>
    <record id="model_ace" model="fleet.vehicle.model">
          <field name="name">ACE</field>
          <field name="brand_id" ref="brand_tata"/>
          <field name="vehicle_type">car</field>
          <field name="category_id" ref="model_category_5"/>
          <field name="model_year">2015</field>
          <field name="default_fuel_type">cng</field>
          <field name="transmission">automatic</field>
          <field name="default_co2">205</field>
          <field name="power">200</field>
          <field name="horsepower">2000</field>
    </record>

    <!-- fleet vehicles -->
    <record id="vehicle_7" model="fleet.vehicle">
        <field name="license_plate">AL-18-7865</field>
        <field name="vin_sn">468453445</field>
        <field name="model_id" ref="model_trailer"/>
        <field name="category_id" ref="model_category_7"/>
        <field name="color">Black</field>
        <field name="location">Gandhinagar-India</field>
        <field name="doors">2</field>
        <field name="driver_id" ref="base.partner_demo" />
        <field name="acquisition_date" eval="(DateTime.now() - timedelta(days=336)).strftime('%Y-%m-%d')" />
        <field name="state_id" ref="fleet.fleet_vehicle_state_registered"/>
        <field name="odometer">18000</field>
        <field name="odometer_unit">kilometers</field>
        <field name="car_value">20000</field>
        <field name="model_year">2018</field>
        <field name="horsepower">6000</field>
        <field name="fuel_type">diesel</field>
        <field name="manager_id" ref="base.user_admin"/>
    </record>
    <record id="vehicle_9" model="fleet.vehicle">
        <field name="license_plate">TA-18-2468</field>
        <field name="vin_sn">12312323</field>
        <field name="model_id" ref="model_ace"/>
        <field name="category_id" ref="model_category_5"/>
        <field name="color">White</field>
        <field name="location">Gandhinagar-India</field>
        <field name="doors">2</field>
        <field name="driver_id" ref="base.res_partner_address_15" />
        <field name="acquisition_date" eval="(DateTime.now() - timedelta(days=336)).strftime('%Y-%m-%d')" />
        <field name="state_id" ref="fleet.fleet_vehicle_state_registered"/>
        <field name="odometer">16000</field>
        <field name="odometer_unit">kilometers</field>
        <field name="car_value">25000</field>
        <field name="model_year">2015</field>
        <field name="horsepower">2000</field>
        <field name="fuel_type">cng</field>
        <field name="manager_id" ref="base.user_admin"/>
    </record>
</odoo>
