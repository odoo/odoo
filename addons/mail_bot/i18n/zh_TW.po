#. odoo-python
#: code:addons/mail_bot/models/mail_bot.py:0
msgid "fuck"
msgstr "嘖嘖!"

#. module: mail_bot
#. odoo-python
#: code:addons/mail_bot/models/mail_bot.py:0
msgid "help"
msgstr "幫助"

#. module: mail_bot
#. odoo-python
#: code:addons/mail_bot/models/mail_bot.py:0
msgid "i love you"
msgstr "我愛你"

#. module: mail_bot
#. odoo-python
#: code:addons/mail_bot/models/mail_bot.py:0
msgid "love"
msgstr "愛"

