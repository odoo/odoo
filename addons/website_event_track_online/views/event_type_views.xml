<?xml version="1.0"?>
<odoo><data>
    <record id="event_type_view_form" model="ir.ui.view">
        <field name="name">event.type.view.form.inherit.meet</field>
        <field name="model">event.type</field>
        <field name="inherit_id" ref="website_event.event_type_view_form_inherit_website"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@name='website_menu']" position='after'>
                <div id="community-menu" class="row mt16" groups="base.group_no_one" attrs="{'invisible': [('website_menu', '=', False)]}">
                    <label class="col-lg-4" for="community_menu"/> <field name="community_menu"/>
                </div>
            </xpath>
        </field>
    </record>

</data></odoo>
