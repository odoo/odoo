<?xml version="1.0"?>
<odoo><data>

    <record id="event_track_stage_view_form" model="ir.ui.view">
        <field name="name">event.track.stage.view.form.inherit.online</field>
        <field name="model">event.track.stage</field>
        <field name="inherit_id" ref="website_event_track.event_track_stage_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='is_done']" position="before">
                <field name="is_accepted"/>
            </xpath>
        </field>
    </record>

    <record id="event_track_stage_view_tree" model="ir.ui.view">
        <field name="name">event.track.stage.view.tree.inherit.online</field>
        <field name="model">event.track.stage</field>
        <field name="inherit_id" ref="website_event_track.event_track_stage_view_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='name']" position="after">
                <field name="is_accepted"/>
                <field name="is_done"/>
                <field name="is_cancel"/>
            </xpath>
        </field>
    </record>

</data></odoo>
