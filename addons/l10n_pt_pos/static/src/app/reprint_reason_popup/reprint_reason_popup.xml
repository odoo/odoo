<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

    <t t-name="l10n_pt_pos.ReprintReasonPopup">
        <Dialog title.translate="Reprint Reason">
            <div class="mb-3">
                <label for="reprint_reason">Reprint Reason:</label>
                <input type="text" name="reprint_reason" t-model="state.reprint_reason" t-att-value="reprint_reason"
                       id="reprint_reason" class="form-control form-control-sm" autofocus="autofocus" required="required"/>
            </div>
            <t t-set-slot="footer">
                <button class="btn btn-primary o-default-button" t-on-click="confirm">Ok</button>
            </t>
        </Dialog>
    </t>

</templates>
