        <field name="category_id" ref="base.module_category_website"/>
    </record>

    <record id="group_website_blog_manager" model="res.groups">
        <field name="name">Blog Manager</field>
        <field name="implied_ids" eval="[(4, ref('website.group_website_restricted_editor'))]"/>
