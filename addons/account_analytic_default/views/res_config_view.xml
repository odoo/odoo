<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="account_config_settings_view_form_inherit_analytic_default" model="ir.ui.view">
        <field name="name">account.config.settings.form.inherit.analytic.default</field>
        <field name="model">account.config.settings</field>
        <field name="inherit_id" ref="account.account_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@id='analytic_account_link']" position="after">
                <div>
                    <button name="%(account_analytic_default.action_analytic_default_list)d" icon="fa-arrow-right" type="action" string="Default Analytic Values" class="btn-link"/>
                </div>
            </xpath>
        </field>
    </record>

</odoo>

