        <field name="code">OMAK</field>
        <field name="level" eval="2"/>
        <field name="sequence" eval="7"/>
        <field name="formulas">balance = VABE.balance + IGANG.balance + FORUD.balance + TILG.balance + ATIL.balance + LIKBE.balance</field>
    </record>
    
