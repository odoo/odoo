<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Have a resolution of 1280x720 (with minimum width of 640 pixels).
    Be uploaded in image formats such as JPG, GIF, or PNG.
    Remain under 2MB for videos or 10MB for podcasts.
    https://support.google.com/youtube/answer/72431#zippy=%2Cimage-size-resolution -->
    <record id="card_dimension_youtube_thumbnail" model="card.dimension">
        <field name="name">YouTube Thumbnail (16:9)</field>
        <field name="width">1280</field>
        <field name="height">720</field>
    </record>
</odoo>
