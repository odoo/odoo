            <field name="sequence" eval="4"/>
            <field name="parent_id" ref='account_financial_report_executivesummary_profitability0'/>
            <field name="formulas">balance = LEX.balance</field>
            <field name="green_on_positive" eval="False"/>
            <field name="level" eval="2" />
        </record>
        <record id="account_financial_report_executivesummary_profit0" model="account.financial.report.line">
            <field name="name">Profit</field>
            <field name="sequence" eval="5"/>
            <field name="parent_id" ref='account_financial_report_executivesummary_profitability0'/>
            <field name="formulas">balance = NEP.balance</field>
            <field name="level" eval="2" />
        </record>
        <record id="account_financial_report_executivesummary_balancesheet0" model="account.financial.report.line">
