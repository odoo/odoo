            <field name="code">DEB</field>
            <field name="sequence" eval="1"/>
            <field name="parent_id" ref='account_financial_report_executivesummary_balancesheet0'/>
            <field name="domain" eval="[('account_id.user_type.type', '=', 'receivable')]" />
