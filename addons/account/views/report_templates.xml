<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="report_invoice_wizard_iframe">
        <t t-call="web.html_preview_container">
            <t t-call="account.report_invoice_document_preview"/>
        </t>
    </template>

    <template id="report_invoice_document_preview" inherit_id="account.report_invoice_document" primary="True">
        <xpath expr="//div[@id='qrcode']" position="after">
            <div t-if="qr_code" class="d-flex mb-3 avoid-page-break-inside">
                <div class="qrcode me-3" id="qrcode_image">
                    <p class="position-relative mb-0">
                        <img src="data:image/png;base64,"/>
                        <img src="/account/static/src/img/Odoo_logo_O.svg"
                             id="qrcode_odoo_logo"
                             class="top-50 start-50 position-absolute bg-white border border-white border-3 rounded-circle"
                        />
                    </p>
                </div>
                <div class="text-muted lh-sm fst-italic" id="qrcode_info">
                    <p>Scan this QR Code to<br/>pay with your mobile</p>
                    <p>IBAN: BE76 4294 6878 9995</p>
                </div>
            </div>
        </xpath>
    </template>
</odoo>
