"o-mail-quote-container=\"1\">\n"
"            <br/><br/>\n"
"            <div data-o-mail-quote=\"1\">--<br data-o-mail-quote=\"1\"/><t t-"
"out=\"object.invoice_user_id.signature or ''\" data-o-mail-"
"quote=\"1\">Mitchell Admin</t></div>\n"
"        </t>\n"
"    </p>\n"
"</div>\n"
"            "
