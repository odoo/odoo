"            <t t-out=\"object.partner_id.name or ''\">Brandon Freeman</t> (<t t-out=\"object.partner_id.parent_id.name or ''\">Azure Interior</t>)님,\n"
"        </t>\n"
"        <t t-else=\"\">\n"
"            <t t-out=\"object.partner_id.name or ''\">Brandon Freeman</t>님,\n"
"        </t>\n"
