"        <t t-else=\"\">\n"
"            <t t-out=\"object.partner_id.name or ''\">Brandon Freeman</t>,\n"
"        </t>\n"
"        <br><br>\n"
"        Here is your\n"
"        <t t-if=\"object.name\">\n"
"            credit note <span style=\"font-weight:bold;\" t-out=\"object.name or ''\">RINV/2021/05/0001</span>\n"
"        </t>\n"
"        <t t-else=\"\">\n"
"            credit note\n"
"        </t>\n"
"        <t t-if=\"object.invoice_origin\">\n"
