<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <t t-name="project.ProjectTaskCalendarFilterSection.filter" t-inherit="web.CalendarFilterSection.filter" t-inherit-mode="primary">
        <xpath expr="//span[@t-esc='this.filter.label']" position="before">
            <span t-if="this.props.model.isCheckProject and !this.filter.value" class="text-danger pe-1">
                <i class="fa fa-lock"/>
            </span>
        </xpath>
    </t>

</templates>
