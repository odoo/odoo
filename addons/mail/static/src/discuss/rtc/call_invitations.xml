<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <t t-name="mail.CallInvitations" owl="1">
        <div class="o-mail-CallInvitations position-absolute top-0 end-0 d-flex flex-column p-2">
            <t t-if="rtc.ringingThreads">
                <t t-foreach="rtc.ringingThreads" t-as="threadLocalId" t-key="threadLocalId">
                    <t t-set="channel" t-value="discussStore.channels[threadLocalId]"/>
                    <CallInvitation t-if="channel" channel="channel"/>
                </t>
            </t>
        </div>
    </t>

</templates>
