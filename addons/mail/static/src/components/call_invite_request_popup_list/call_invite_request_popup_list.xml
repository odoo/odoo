<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <t t-name="mail.CallInviteRequestPopupListView" owl="1">
        <div class="o_CallInviteRequestPopupListView position-absolute top-0 end-0 d-flex flex-column p-2" t-attf-class="{{ className }}" t-ref="root">
            <t t-if="messaging.ringingThreads">
                <t t-foreach="messaging.callInviteRequestPopupViews" t-as="callInviteRequestPopupView" t-key="callInviteRequestPopupView.localId">
                    <CallInviteRequestPopupView record="callInviteRequestPopupView"/>
                </t>
            </t>
        </div>
    </t>

</templates>
