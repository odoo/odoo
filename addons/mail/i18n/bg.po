msgid ""
"\n"
"<div itemscope itemtype=\"http://schema.org/EmailMessage\">\n"
"    <div itemprop=\"potentialAction\" itemscope itemtype=\"http://schema.org/ViewAction\">\n"
"        % if ctx.get('button_access'):\n"
