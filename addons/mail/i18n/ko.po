"                % if ctx.get('actions'):\n"
"                   |\n"
"                % endif\n"
"                <a href=\"${ctx['button_access']['url']}\" style=\"text-"
"decoration:none; color: #a24689;\"><strong>${ctx['button_access']['title']}</"
