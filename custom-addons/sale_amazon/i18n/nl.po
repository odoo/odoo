msgid ""
"<style>\n"
"                .amz_table {\n"
"                    width: 100%;\n"
"                    max-width: 900px;\n"
"                    border-collapse: collapse;\n"
"                }\n"
"\n"
"                .amz_table th,\n"
"                .amz_table td {\n"
"                    border: 1px solid #888;\n"
"                }\n"
"\n"
"                .amz_table th {\n"
