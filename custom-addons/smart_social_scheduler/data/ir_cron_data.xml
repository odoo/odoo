<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Cron job to process social post queue -->
        <record id="ir_cron_process_social_queue" model="ir.cron">
            <field name="name">Process Social Post Queue</field>
            <field name="model_id" ref="model_social_post_queue"/>
            <field name="state">code</field>
            <field name="code">model.cron_process_queue()</field>
            <field name="interval_number">15</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="active">True</field>
        </record>
    </data>
</odoo>

