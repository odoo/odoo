<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Demo Products with eCommerce Extension Fields -->
        
        <!-- Samsung Galaxy Phone -->
        <record id="demo_product_samsung_s24" model="product.template">
            <field name="name">Samsung Galaxy S24 Ultra</field>
            <field name="type">consu</field>
            <field name="is_storable" eval="True"/>
            <field name="list_price">45000</field>
            <field name="brand">Samsung</field>
            <field name="product_model">SM-S928B</field>
            <field name="shipping_weight_kg">0.35</field>
            <field name="estimated_delivery_days">2</field>
            <field name="low_stock_threshold">5</field>
            <field name="is_published" eval="True"/>
            <field name="sale_ok" eval="True"/>
            <field name="specifications"><![CDATA[
                <table class="table table-striped">
                    <tr><th>Display</th><td>6.8" Dynamic AMOLED 2X, 3088x1440</td></tr>
                    <tr><th>Processor</th><td>Snapdragon 8 Gen 3</td></tr>
                    <tr><th>RAM</th><td>12GB</td></tr>
                    <tr><th>Storage</th><td>256GB / 512GB / 1TB</td></tr>
                    <tr><th>Battery</th><td>5000mAh, 45W Fast Charging</td></tr>
                    <tr><th>Camera</th><td>200MP Main + 12MP Ultra Wide + 50MP Telephoto + 10MP Periscope</td></tr>
                    <tr><th>S Pen</th><td>Included</td></tr>
                </table>
            ]]></field>
            <field name="video_url">https://www.youtube.com/watch?v=example</field>
        </record>
        
