                                                <field name="name">Other Provisions</field>
                                                <field name="code">NO_216_218</field>
                                                <field name="groupby">account_id</field>
                                                <field name="foldable" eval="True"/>
                                                <field name="account_codes_formula">-218 - 216 - 214</field>
                                            </record>
                                        </field>
                                    </record>
