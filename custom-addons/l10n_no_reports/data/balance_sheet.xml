                        <field name="code">NO_ACTIVE</field>
                        <field name="aggregation_formula">NO_ACTIVE_A.balance + NO_ACTIVE_B.balance</field>
                    </record>
                    <record id="account_financial_report_NO_active_FA" model="account.report.line">
