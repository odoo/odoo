<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="partner_demo_company_km" model="res.partner">
        <field name="name">KM Company</field>
        <field name="vat"></field>
        <field name="street"></field>
        <field name="city"></field>
        <field name="country_id" ref="base.km"/>

        <field name="zip"></field>
        <field name="phone">+269 316 52 61</field>
        <field name="email">info@company.comorosexample.com</field>
        <field name="website">www.comorosexample.com</field>
    </record>

    <record id="demo_company_km" model="res.company">
        <field name="name">KM Company</field>
        <field name="partner_id" ref="partner_demo_company_km"/>
    </record>

    <function model="res.company" name="_onchange_country_id">
        <value eval="[ref('demo_company_km')]"/>
    </function>

    <function model="res.users" name="write">
        <value eval="[ref('base.user_root'), ref('base.user_admin'), ref('base.user_demo')]"/>
        <value eval="{'company_ids': [(4, ref('l10n_km.demo_company_km'))]}"/>
    </function>

    <function model="account.chart.template" name="try_loading">
        <value eval="[]"/>
        <value>km</value>
        <value model="res.company" eval="obj().env.ref('l10n_km.demo_company_km')"/>
    </function>
</odoo>
