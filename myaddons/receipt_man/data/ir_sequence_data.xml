<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for sale.order -->
        <record id="cf_receipt_product" model="ir.sequence">
            <field name="name">CF Receipt Product</field>
            <field name="code">cf.receipt.product</field>
            <field name="prefix">P</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for sale.order -->
        <record id="cf_receipt_godown_entry" model="ir.sequence">
            <field name="name">CF Receipt Godown Entry</field>
            <field name="code">cf.receipt.godown.entry</field>
            <field name="prefix">GE%(year)s%(month)s%(day)s</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for sale.order -->
        <record id="cf_receipt_delivery_order" model="ir.sequence">
            <field name="name">CF Receipt Delivery Order</field>
            <field name="code">cf.receipt.delivery.order</field>
            <field name="prefix">DO%(year)s%(month)s%(day)s</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
