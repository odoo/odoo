odoo.define('@portal/interactions/portal_composer',['@web/public/interaction','@web/core/registry','@web/core/l10n/translation','@web/core/network/http_service','@odoo/owl','@web/core/network/rpc'],function(require){'use strict';let __exports={};const{Interaction}=require("@web/public/interaction");const{registry}=require("@web/core/registry");const{_t}=require("@web/core/l10n/translation");const{post}=require("@web/core/network/http_service");const{Component,markup}=require("@odoo/owl");const{rpc,RPCError}=require("@web/core/network/rpc");const PortalComposer=__exports.PortalComposer=class PortalComposer extends Interaction{static selector=".o_portal_chatter_composer";static selectorHas=".o_portal_chatter_composer_input";dynamicContent={".o_portal_chatter_file_input":{"t-on-change":this.onFileInputChange,},".o_portal_chatter_attachment_btn":{"t-on-click":this.onAttachmentButtonClick,},".o_portal_chatter_attachment_delete":{"t-on-click.prevent.stop.withTarget":this.locked(this.onAttachmentDeleteClick,true),},".o_portal_chatter_composer_btn":{"t-on-click.prevent.withTarget":this.locked(this.onSubmitButtonClick,true),},};static prepareOptions(options){if(typeof options.default_attachment_ids==="string"){options.default_attachment_ids=JSON.parse(options.default_attachment_ids);}
