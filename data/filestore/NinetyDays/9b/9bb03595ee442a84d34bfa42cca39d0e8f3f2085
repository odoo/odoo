function unregister(){workerService.send("ELECTION:UNREGISTER");state=STATE.UNREGISTERED;}
return{bus,isOnMainTab:async()=>{if(state===STATE.UNREGISTERED){return false;}
if(state===STATE.INIT){await startWorker();}
