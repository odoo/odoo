registry.category("public.interactions").add("portal.portal_security",PortalSecurity);__exports.handleCheckIdentity=handleCheckIdentity;async function handleCheckIdentity(wrapped,ormService,dialogService){return wrapped.then((r)=>{if(!(r&&r.type&&r.type==="ir.actions.act_window"&&r.res_model==="res.users.identitycheck")){return r;}
