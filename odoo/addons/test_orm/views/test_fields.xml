<odoo>
    <data>
        <record id="test_fields_model_shared_cache_compute_parent_view_form" model="ir.ui.view">
            <field name="name">test_fields_model_shared_cache_compute_parent_view_form</field>
            <field name="model">test_fields.model_shared_cache_compute_parent</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="total_amount"/>
                            <field name="line_ids">
                                <list editable="bottom">
                                    <field name="parent_id"/>
                                    <field name="amount"/>
                                    <field name="user_id"/>
                                </list>
                            </field>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>