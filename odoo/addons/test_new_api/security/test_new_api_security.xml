<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <record id="ir_rule_test_new_api_model_shared_cache_compute_line" model="ir.rule">
            <field name="name">model_shared_cache_compute_line: See own lines</field>
            <field name="model_id" ref="test_new_api.model_test_new_api_model_shared_cache_compute_line"/>
            <field name="domain_force">[('user_id', '=', user.id)]</field>
            <field name="groups" eval="[Command.link(ref('base.group_user'))]"/>
        </record>

        <record id="ir_rule_test_new_api_model_simple_task" model="ir.rule">
            <field name="name">model_simple_task: Restrict all access to owner if it is set</field>
            <field name="model_id" ref="test_new_api.model_test_new_api_simple_task"/>
            <field name="domain_force">['|', ('user_id', '=', user.id), ('user_id', '=', False)]</field>
            <field name="groups" eval="[Command.link(ref('base.group_user'))]"/>
        </record>
    </data>
</odoo>
