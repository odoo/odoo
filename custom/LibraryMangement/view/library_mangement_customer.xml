<odoo>
    <record id="library_mangement_customer_view_form" model="ir.ui.view">
        <field name="name">library.mangement.customer.form</field>
        <field name="model">library.mangement.customer</field>
        <field name="arch" type="xml">
            <form string="book">
                <sheet>
                    <group >
                        <field name="name"/>
                        <field name="age"/>
                    </group>
                    <notebook>
                        <page string="Borrow History">
                            <list>
                                <field name="borrow_list"/>
                            </list>
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>

    <record id="library_mangement_customer_view_list" model="ir.ui.view">
        <field name="name">library.mangement.customer.list</field>
        <field name="model">library.mangement.customer</field>
        <field name="arch" type="xml">
            <list >
                <field name="name"/>
                <field name="age"/>
            </list>
        </field>
    </record>
</odoo>