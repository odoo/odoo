<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="marcos_view_warehouse" model="ir.ui.view">
            <field name="name">marcos.stock.warehouse</field>
            <field name="model">stock.warehouse</field>
            <field name="inherit_id" ref="stock.view_warehouse"/>
            <field name="arch" type="xml">
                <xpath expr="//form[@string='Warehouse']//sheet" position="inside">
                    <group>
                        <field name="fiscal_id"/>
                        <field name="final_id"/>
                        <field name="especiales_id"/>
                        <field name="gubernamentales_id"/>
                        <field name="notas_credito_id"/>
                        <field name="default_partner_id"/>
                        <field name="default_receipt_journal_id"/>
                    </group>
                </xpath>
            </field>
        </record>
    </data>
</openerp>